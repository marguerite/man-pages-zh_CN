#: efibootmgr.8.xml:177(option)
msgid "-O | --delete-bootorder"
msgstr "-O | --delete-bootorder"

#: efibootmgr.8.xml:179(para)
msgid "Delete BootOrder"
msgstr "删除引导顺序"

#: efibootmgr.8.xml:183(option)
msgid "-p | --part"
msgstr "-p | --part"

#: efibootmgr.8.xml:185(para)
msgid "Partition number containing the bootloader (defaults to 1)"
msgstr "包含引导加载器的分区编号 (默认为 1)"

#: efibootmgr.8.xml:189(option)
msgid "-q | --quiet"
msgstr "-q | --quiet"

#: efibootmgr.8.xml:191(para)
msgid "Quiet mode - supresses output."
msgstr "静默模式 - 压制输出。"

#: efibootmgr.8.xml:195(option)
msgid "--test"
msgstr "--test"

#: efibootmgr.8.xml:195(emphasis)
msgid "filename"
msgstr "文件名"

