msgstr "设置 BootNext 为 XXXX (十六进制)"

#: efibootmgr.8.xml:165(option)
msgid "-N | --delete-bootnext"
msgstr "-N | --delete-bootnext"

#: efibootmgr.8.xml:167(para)
msgid "Delete BootNext"
msgstr "删除 BootNext"

#: efibootmgr.8.xml:171(option)
