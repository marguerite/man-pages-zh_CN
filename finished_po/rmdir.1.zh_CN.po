#: rmdir.1.xml:8(refentrytitle)
msgid "RMDIR"
msgstr "rmdir"

#: rmdir.1.xml:9(manvolnum)
msgid "1"
msgstr "1"

#: rmdir.1.xml:10(refmiscinfo)
msgid "July 2014"
msgstr "2014 年 7 月"

#: rmdir.1.xml:11(refmiscinfo)
msgid "GNU coreutils 8.22"
msgstr "GNU coreutils 8.22"

#: rmdir.1.xml:14(refname) rmdir.1.xml:20(command)
#: rmdir.1.xml:83(refentrytitle)
msgid "rmdir"
msgstr "rmdir"

#: rmdir.1.xml:15(refpurpose)
msgid "remove empty directories"
msgstr "移除空文件夹"

#: rmdir.1.xml:21(replaceable)
msgid "OPTION"
msgstr "选项"

#: rmdir.1.xml:22(replaceable)
msgid "DIRECTORY"
msgstr "文件夹"

#: rmdir.1.xml:28(title)
msgid "DESCRIPTION"
msgstr "描述"

#. Add any additional description here
#: rmdir.1.xml:31(para)
msgid "Remove the DIRECTORY(ies), if they are empty."
msgstr "移除空文件夹。"

#: rmdir.1.xml:33(option)
msgid "--ignore-fail-on-non-empty"
msgstr "--ignore-fail-on-non-empty"

#: rmdir.1.xml:35(para)
msgid "ignore each failure that is solely because a directory"
msgstr "忽略文件夹非空的失败"

#: rmdir.1.xml:37(para)
msgid "is non-empty"
msgstr "非空"

#: rmdir.1.xml:40(term)
msgid "<option>-p</option>, <option>--parents</option>"
msgstr "<option>-p</option>, <option>--parents</option>"

#: rmdir.1.xml:42(para)
msgid ""
"remove DIRECTORY and its ancestors; e.g., 'rmdir <option>-p</option> a/b/c' "
"is similar to 'rmdir a/b/c a/b a'"
msgstr ""
"移除文件夹和其上级文件夹; 例如，'rmdir <option>-p</option> a/b/c' "
"等价于 'rmdir a/b/c a/b a'"

#: rmdir.1.xml:47(term)
msgid "<option>-v</option>, <option>--verbose</option>"
msgstr "<option>-v</option>, <option>--verbose</option>"

#: rmdir.1.xml:49(para)
msgid "output a diagnostic for every directory processed"
msgstr "为每个处理的文件夹输出诊断信息"

#: rmdir.1.xml:53(option)
msgid "--help"
msgstr "--help"

#: rmdir.1.xml:55(para)
msgid "display this help and exit"
msgstr "显示此帮助并退出"

#: rmdir.1.xml:59(option)
msgid "--version"
msgstr "--version"

#: rmdir.1.xml:61(para)
msgid "output version information and exit"
msgstr "输出版本信息并退出"

#: rmdir.1.xml:63(para)
msgid ""
"GNU coreutils online help: &lt;<ulink url=\"http://www.gnu.org/software/"
"coreutils/&gt;\">http://www.gnu.org/software/coreutils/&gt;</ulink> Report "
"rmdir translation bugs to &lt;<ulink url=\"http://translationproject.org/"
"team/&gt;\">http://translationproject.org/team/&gt;</ulink>"
msgstr ""
"GNU coreutils 在线帮助：&lt;<ulink url=\"http://www.gnu.org/software/"
"coreutils/&gt;\">http://www.gnu.org/software/coreutils/&gt;</ulink> 汇报 "
"rmdir 翻译故障 &lt;<ulink url=\"http://translationproject.org/"
"team/&gt;\">http://translationproject.org/team/&gt;</ulink>"

