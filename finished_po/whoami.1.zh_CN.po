#: whoami.1.xml:8(refentrytitle)
msgid "WHOAMI"
msgstr "whoami"

#: whoami.1.xml:9(manvolnum)
msgid "1"
msgstr "1"

#: whoami.1.xml:10(refmiscinfo)
msgid "July 2014"
msgstr "2014 年 7 月"

#: whoami.1.xml:11(refmiscinfo)
msgid "GNU coreutils 8.22"
msgstr "GNU coreutils 8.22"

#: whoami.1.xml:14(refname) whoami.1.xml:20(command)
msgid "whoami"
msgstr "whoami"

#: whoami.1.xml:15(refpurpose)
msgid "print effective userid"
msgstr "输出有效用户 ID"

#: whoami.1.xml:21(replaceable)
msgid "OPTION"
msgstr "选项"

#: whoami.1.xml:27(title)
msgid "DESCRIPTION"
msgstr "描述"

#. Add any additional description here
#: whoami.1.xml:30(para)
msgid ""
"Print the user name associated with the current effective user ID. Same as "
"id <option>-un</option>."
msgstr "输出与当前有效用户 ID 对应的用户名。与 id <option>-un</option> 一样。"

#: whoami.1.xml:34(option)
msgid "--help"
msgstr "--help"

#: whoami.1.xml:36(para)
msgid "display this help and exit"
msgstr "显示此帮助并退出"

#: whoami.1.xml:40(option)
msgid "--version"
msgstr "--version"

#: whoami.1.xml:42(para)
msgid "output version information and exit"
msgstr "输出版本信息并退出"

#: whoami.1.xml:44(para)
msgid ""
"GNU coreutils online help: &lt;<ulink url=\"http://www.gnu.org/software/"
"coreutils/&gt;\">http://www.gnu.org/software/coreutils/&gt;</ulink> Report "
"whoami translation bugs to &lt;<ulink url=\"http://translationproject.org/"
"team/&gt;\">http://translationproject.org/team/&gt;</ulink>"
msgstr ""
"GNU coreutils 在线帮助：&lt;<ulink url=\"http://www.gnu.org/software/"
"coreutils/&gt;\">http://www.gnu.org/software/coreutils/&gt;</ulink> 汇报 "
"whoami 翻译故障 &lt;<ulink url=\"http://translationproject.org/"
"team/&gt;\">http://translationproject.org/team/&gt;</ulink>"

#: whoami.1.xml:51(title)
msgid "AUTHOR"
msgstr "作者"

#: whoami.1.xml:52(para)
msgid "Written by Richard Mlynarik."
msgstr "由 Richard Mlynarik 编纂。"

