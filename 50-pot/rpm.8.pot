msgstr ""

#: rpm.8.xml:479(option)
msgid "--replacefiles"
msgstr ""

#: rpm.8.xml:481(para)
msgid "Install the packages even if they replace files from other, already installed, packages."
msgstr ""

#: rpm.8.xml:486(option)
msgid "--replacepkgs"
msgstr ""

#: rpm.8.xml:488(para)
msgid "Install the packages even if some of them are already installed on this system."
msgstr ""

#: rpm.8.xml:493(option) rpm.8.xml:563(option)
msgid "--test"
msgstr ""

#: rpm.8.xml:495(para)
msgid "Do not install the package, simply check for and report potential conflicts."
msgstr ""

