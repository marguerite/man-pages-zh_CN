#: rpm.8.xml:399(option) rpm.8.xml:409(option)
msgid "--nopretrans"
msgstr ""

#: rpm.8.xml:400(option) rpm.8.xml:410(option)
msgid "--noposttrans"
msgstr ""

#: rpm.8.xml:402(para)
msgid "Don't execute the scriptlet of the same name. The <option>--noscripts</option> option is equivalent to"
