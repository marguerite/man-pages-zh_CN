#: rpm.8.xml:437(para)
msgid "and turns off execution of the corresponding <emphasis remap=\"B\">%triggerprein</emphasis>, <emphasis remap=\"B\">%triggerin</emphasis>, <emphasis remap=\"B\">%triggerun</emphasis>, and <emphasis remap=\"B\">%triggerpostun</emphasis> scriptlet(s)."
msgstr ""

#: rpm.8.xml:446(option)
msgid "--oldpackage"
msgstr ""

#: rpm.8.xml:448(para)
msgid "Allow an upgrade to replace a newer package with an older one."
msgstr ""

#: rpm.8.xml:452(option)
msgid "--percent"
msgstr ""

#: rpm.8.xml:454(para)
msgid "Print percentages as files are unpacked from the package archive. This is intended to make <emphasis remap=\"B\">rpm</emphasis> easy to run from other tools."
msgstr ""

#: rpm.8.xml:460(option)
msgid "--prefix"
msgstr ""

#: rpm.8.xml:460(emphasis) rpm.8.xml:468(emphasis)
msgid "NEWPATH"
msgstr ""

#: rpm.8.xml:462(para)
msgid "For relocatable binary packages, translate all file paths that start with the installation prefix in the package relocation hint(s) to <emphasis remap=\"I\">NEWPATH</emphasis>."
