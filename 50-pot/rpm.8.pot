msgstr ""

#: rpm.8.xml:802(emphasis)
msgid "SPECFILE"
msgstr ""

#: rpm.8.xml:804(para)
msgid "Parse and query <emphasis remap=\"I\">SPECFILE</emphasis> as if it were a package. Although not all the information (e.g. file lists) is available, this type of query permits rpm to be used to extract information from spec files without having to write a specfile parser."
msgstr ""

#: rpm.8.xml:812(option)
msgid "--tid"
msgstr ""

#: rpm.8.xml:812(emphasis)
msgid "TID"
msgstr ""

#: rpm.8.xml:814(para)
msgid "Query package(s) that have a given <emphasis remap=\"I\">TID</emphasis> transaction identifier. A unix time stamp is currently used as a transaction identifier. All package(s) installed or erased within a single transaction have a common identifier."
