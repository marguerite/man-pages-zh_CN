#: efibootmgr.8.xml:56(title)
msgid "DESCRIPTION"
msgstr ""

#: efibootmgr.8.xml:57(para)
msgid "<command>efibootmgr</command> is a userspace application used to modify the Intel Extensible Firmware Interface (EFI) Boot Manager. This application can create and destroy boot entries, change the boot order, change the next running boot option, and more."
msgstr ""

#: efibootmgr.8.xml:62(para)
msgid "Details on the EFI Boot Manager are available from the EFI Specification, v1.02 or later, available from: &lt;URL:<ulink url=\"http://developer.intel.com&gt;\">http://developer.intel.com&gt;</ulink>"
msgstr ""

#: efibootmgr.8.xml:67(para)
msgid "<emphasis remap=\"B\">Note:</emphasis> efibootmgr requires that the kernel support access to EFI non-volatile variables (through <filename>/proc/efi/vars</filename> on 2.4 kernels, <filename>/sys/firmware/efi/vars</filename> on 2.6 kernels). <emphasis remap=\"B\">modprobe efivars</emphasis> should do the trick."
msgstr ""

#: efibootmgr.8.xml:76(title)
msgid "OPTIONS"
msgstr ""

#: efibootmgr.8.xml:77(para)
msgid "The following is a list of options accepted by efibootmgr:"
msgstr ""

#: efibootmgr.8.xml:80(option)
msgid "-a | --active"
msgstr ""

#: efibootmgr.8.xml:82(para)
msgid "Sets bootnum active"
msgstr ""

#: efibootmgr.8.xml:86(option)
msgid "-A | --inactive"
msgstr ""

#: efibootmgr.8.xml:88(para)
msgid "Sets bootnum inactive"
msgstr ""

#: efibootmgr.8.xml:92(option)
msgid "-b | --bootnum"
msgstr ""

#: efibootmgr.8.xml:94(para)
msgid "Modify Boot<emphasis remap=\"I\">XXXX</emphasis> (hex)"
msgstr ""

#: efibootmgr.8.xml:98(option)
msgid "-B | --delete-bootnum"
msgstr ""

#: efibootmgr.8.xml:100(para)
msgid "Delete bootnum (hex)"
msgstr ""

#: efibootmgr.8.xml:104(option)
msgid "-c | --create"
msgstr ""

#: efibootmgr.8.xml:106(para)
msgid "Create new variable bootnum and add to bootorder"
msgstr ""

#: efibootmgr.8.xml:110(option)
msgid "-d | --disk"
msgstr ""

#: efibootmgr.8.xml:112(para)
msgid "The disk containing the loader (defaults to <filename>/dev/sda</filename>)"
msgstr ""

#: efibootmgr.8.xml:117(option)
msgid "-e | --edd"
msgstr ""

#: efibootmgr.8.xml:117(emphasis)
msgid "1|3|-1"
msgstr ""

#: efibootmgr.8.xml:119(para)
msgid "Force EDD 1.0 or 3.0 creation variables, or guess."
msgstr ""

#: efibootmgr.8.xml:123(option)
msgid "-E | --device"
msgstr ""

#: efibootmgr.8.xml:125(para)
msgid "EDD 1.0 device number (defaults to 0x80)"
msgstr ""

#: efibootmgr.8.xml:129(option)
msgid "-g | --gpt"
msgstr ""

#: efibootmgr.8.xml:131(para)
msgid "Force disk with invalid PMBR to be treated as GPT"
msgstr ""

#: efibootmgr.8.xml:135(option)
msgid "-H | --acpi_hid"
msgstr ""

#: efibootmgr.8.xml:137(para)
msgid "set the ACPI HID (used with <option>-i</option>)"
