msgstr ""

#. .fi
#: npm-config.1.xml:71(para)
msgid "Sets the config key to the value."
msgstr ""

#: npm-config.1.xml:73(para)
msgid "If value is omitted, then it sets it to \"true\"."
msgstr ""

#: npm-config.1.xml:78(literallayout)
#, no-wrap
