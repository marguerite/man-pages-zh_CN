msgstr ""

#: stty.1.xml:103(para)
msgid "alternate CHAR for ending the line"
msgstr ""

#: stty.1.xml:107(term)
msgid "erase CHAR"
msgstr ""

#: stty.1.xml:109(para)
msgid "CHAR will erase the last character typed"
msgstr ""

#: stty.1.xml:113(term)
msgid "intr CHAR"
msgstr ""

#: stty.1.xml:115(para)
