msgid "* lnext CHAR"
msgstr ""

#: stty.1.xml:127(para)
msgid "CHAR will enter the next character quoted"
msgstr ""

#: stty.1.xml:131(term)
msgid "quit CHAR"
msgstr ""

#: stty.1.xml:133(para)
msgid "CHAR will send a quit signal"
