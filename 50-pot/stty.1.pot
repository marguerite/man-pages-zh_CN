#: stty.1.xml:143(term)
msgid "start CHAR"
msgstr ""

#: stty.1.xml:145(para)
msgid "CHAR will restart the output after stopping it"
msgstr ""

#: stty.1.xml:149(term)
msgid "stop CHAR"
msgstr ""

#: stty.1.xml:151(para)
msgid "CHAR will stop the output"
msgstr ""

#: stty.1.xml:155(term)
msgid "susp CHAR"
msgstr ""

#: stty.1.xml:157(para)
msgid "CHAR will send a terminal stop signal"
msgstr ""

#: stty.1.xml:161(term)
msgid "* swtch CHAR"
msgstr ""

#: stty.1.xml:163(para)
msgid "CHAR will switch to a different shell layer"
msgstr ""

#: stty.1.xml:167(term)
msgid "* werase CHAR"
msgstr ""

#: stty.1.xml:169(para)
msgid "CHAR will erase the last word typed"
msgstr ""

#: stty.1.xml:175(title)
msgid "Special settings:"
msgstr ""

#: stty.1.xml:178(term)
msgid "N"
msgstr ""

#: stty.1.xml:180(para)
msgid "set the input and output speeds to N bauds"
msgstr ""

#: stty.1.xml:184(term)
msgid "* cols N"
msgstr ""

#: stty.1.xml:186(para)
msgid "tell the kernel that the terminal has N columns"
msgstr ""

#: stty.1.xml:190(term)
msgid "* columns N"
msgstr ""

#: stty.1.xml:192(para)
msgid "same as cols N"
msgstr ""

#: stty.1.xml:196(term)
msgid "ispeed N"
msgstr ""

#: stty.1.xml:198(para)
msgid "set the input speed to N"
msgstr ""

#: stty.1.xml:202(term)
msgid "* line N"
msgstr ""

#: stty.1.xml:204(para)
msgid "use line discipline N"
msgstr ""

#: stty.1.xml:208(term)
msgid "min N"
msgstr ""

#: stty.1.xml:210(para)
msgid "with <option>-icanon</option>, set N characters minimum for a completed read"
msgstr ""

#: stty.1.xml:214(term)
msgid "ospeed N"
msgstr ""

#: stty.1.xml:216(para)
msgid "set the output speed to N"
msgstr ""

#: stty.1.xml:220(term)
msgid "* rows N"
msgstr ""

#: stty.1.xml:222(para)
msgid "tell the kernel that the terminal has N rows"
msgstr ""

#: stty.1.xml:226(term)
msgid "* size"
msgstr ""

#: stty.1.xml:228(para)
msgid "print the number of rows and columns according to the kernel"
msgstr ""

#: stty.1.xml:232(term)
msgid "speed"
msgstr ""

#: stty.1.xml:234(para)
msgid "print the terminal speed"
msgstr ""

#: stty.1.xml:238(term)
msgid "time N"
msgstr ""

#: stty.1.xml:240(para)
msgid "with <option>-icanon</option>, set read timeout of N tenths of a second"
msgstr ""

#: stty.1.xml:246(title)
msgid "Control settings:"
msgstr ""

#: stty.1.xml:249(term)
msgid "[-]clocal"
msgstr ""

#: stty.1.xml:251(para)
msgid "disable modem control signals"
msgstr ""

#: stty.1.xml:255(term)
msgid "[-]cread"
msgstr ""

#: stty.1.xml:257(para)
msgid "allow input to be received"
msgstr ""

#: stty.1.xml:261(term)
msgid "* [-]crtscts"
msgstr ""

#: stty.1.xml:263(para)
msgid "enable RTS/CTS handshaking"
msgstr ""

#: stty.1.xml:267(term)
msgid "* [-]cdtrdsr"
msgstr ""

#: stty.1.xml:269(para)
msgid "enable DTR/DSR handshaking"
msgstr ""

#: stty.1.xml:273(term)
msgid "csN"
msgstr ""

#: stty.1.xml:275(para)
msgid "set character size to N bits, N in [5..8]"
msgstr ""

#: stty.1.xml:279(term)
msgid "[-]cstopb"
msgstr ""

#: stty.1.xml:281(para)
msgid "use two stop bits per character (one with '-')"
msgstr ""

#: stty.1.xml:285(term)
msgid "[-]hup"
msgstr ""

#: stty.1.xml:287(para)
msgid "send a hangup signal when the last process closes the tty"
msgstr ""

#: stty.1.xml:291(term)
msgid "[-]hupcl"
msgstr ""

#: stty.1.xml:293(para)
msgid "same as [-]hup"
msgstr ""

#: stty.1.xml:297(term)
msgid "[-]parenb"
msgstr ""

#: stty.1.xml:299(para)
msgid "generate parity bit in output and expect parity bit in input"
msgstr ""

#: stty.1.xml:303(term)
msgid "[-]parodd"
msgstr ""

#: stty.1.xml:305(para)
msgid "set odd parity (or even parity with '-')"
msgstr ""

#: stty.1.xml:309(term)
msgid "* [-]cmspar"
msgstr ""

#: stty.1.xml:311(para)
msgid "use \"stick\" (mark/space) parity"
msgstr ""

