#: npm-help-search.1.xml:12(refname)
msgid "npm-help-search"
msgstr ""

#: npm-help-search.1.xml:13(refpurpose)
msgid "Search npm help documentation"
msgstr ""

#: npm-help-search.1.xml:17(para)
msgid "npm help-search &lt;text&gt;"
msgstr ""

#: npm-help-search.1.xml:21(title)
msgid "DESCRIPTION"
msgstr ""

#: npm-help-search.1.xml:22(para)
msgid "This command will search the npm markdown documentation files for the terms provided, and then list the results, sorted by relevance."
msgstr ""

#: npm-help-search.1.xml:25(para)
msgid "If only one result is found, then it will show that help topic."
msgstr ""

#: npm-help-search.1.xml:27(para)
msgid "If the argument to <emphasis remap=\"B\">npm help</emphasis> is not a known help topic, then it will call <emphasis remap=\"B\">help-search</emphasis>&thinsp;. It is rarely if ever necessary to call this command directly."
msgstr ""

#: npm-help-search.1.xml:32(title)
msgid "CONFIGURATION"
msgstr ""

#: npm-help-search.1.xml:34(title)
msgid "long"
msgstr ""

#: npm-help-search.1.xml:38(para)
msgid "Type: Boolean"
msgstr ""

#: npm-help-search.1.xml:41(para)
msgid "Default false"
msgstr ""

#. remap='RE'
#: npm-help-search.1.xml:47(para)
msgid "If true, the \"long\" flag will cause help-search to output context around where the terms were found in the documentation."
msgstr ""

#: npm-help-search.1.xml:50(para)
msgid "If false, then help-search will just list out the help topics found."
msgstr ""

