#: conky.1.xml:3549(emphasis)
msgid "tcp_portmon"
msgstr ""

#: conky.1.xml:3549(emphasis)
msgid "port_begin port_end item (index)"
msgstr ""

#: conky.1.xml:3551(para)
msgid "TCP port (both IPv6 and IPv4) monitor for specified local ports. Port numbers must be in the range 1 to 65535. Valid items are:"
msgstr ""

#: conky.1.xml:3555(emphasis)
msgid "count"
msgstr ""

#: conky.1.xml:3556(emphasis)
msgid "Total number of connections in the range"
msgstr ""

#. br
#: conky.1.xml:3559(emphasis)
msgid "rip"
msgstr ""

#: conky.1.xml:3560(emphasis)
msgid "Remote ip address"
msgstr ""

#. br
#: conky.1.xml:3562(emphasis)
msgid "rhost"
msgstr ""

#: conky.1.xml:3563(emphasis)
msgid "Remote host name"
msgstr ""

#. br
#: conky.1.xml:3565(emphasis)
msgid "rport"
msgstr ""

#: conky.1.xml:3566(emphasis)
msgid "Remote port number"
msgstr ""

#. br
#: conky.1.xml:3568(emphasis)
msgid "rservice"
msgstr ""

#: conky.1.xml:3569(emphasis)
msgid "Remote service name from /etc/services"
msgstr ""

#. br
#: conky.1.xml:3572(emphasis)
msgid "lip"
msgstr ""

#: conky.1.xml:3573(emphasis)
msgid "Local ip address"
msgstr ""

#. br
#: conky.1.xml:3575(emphasis)
msgid "lhost"
msgstr ""

#: conky.1.xml:3576(emphasis)
msgid "Local host name"
msgstr ""

#. br
#: conky.1.xml:3578(emphasis)
msgid "lport"
msgstr ""

#: conky.1.xml:3579(emphasis)
msgid "Local port number"
msgstr ""

#. br
#: conky.1.xml:3581(emphasis)
msgid "lservice"
msgstr ""

#: conky.1.xml:3582(emphasis)
msgid "Local service name from /etc/services"
msgstr ""

#: conky.1.xml:3585(para)
msgid "The connection index provides you with access to each connection in the port monitor. The monitor will return information for index values from 0 to n-1 connections. Values higher than n-1 are simply ignored. For the \"count\" item, the connection index must be omitted. It is required for all other items."
msgstr ""

#: conky.1.xml:3592(para)
msgid "Examples:"
msgstr ""

#: conky.1.xml:3594(emphasis)
msgid "${tcp_portmon 6881 6999 count}"
msgstr ""

#: conky.1.xml:3596(emphasis)
msgid "Displays the number of connections in the bittorrent port range"
msgstr ""

#. br
#: conky.1.xml:3599(emphasis)
msgid "${tcp_portmon 22 22 rip 0}"
msgstr ""

#: conky.1.xml:3600(emphasis)
msgid "Displays the remote host ip of the first sshd connection"
msgstr ""

#. br
#: conky.1.xml:3603(emphasis)
msgid "${tcp_portmon 22 22 rip 9}"
msgstr ""

#: conky.1.xml:3604(emphasis)
msgid "Displays the remote host ip of the tenth sshd connection"
msgstr ""

#. br
#: conky.1.xml:3607(emphasis)
msgid "${tcp_portmon 1 1024 rhost 0}"
msgstr ""

#: conky.1.xml:3609(emphasis)
msgid "Displays the remote host name of the first connection on a privileged port"
msgstr ""

#. br
#: conky.1.xml:3612(emphasis)
msgid "${tcp_portmon 1 1024 rport 4}"
msgstr ""

#: conky.1.xml:3614(emphasis)
msgid "Displays the remote host port of the fifth connection on a privileged port"
msgstr ""

#. br
#: conky.1.xml:3617(emphasis)
msgid "${tcp_portmon 1 65535 lservice 14}"
msgstr ""

#: conky.1.xml:3619(emphasis)
msgid "Displays the local service name of the fifteenth connection in the range of all ports"
msgstr ""

#: conky.1.xml:3623(para)
msgid "Note that port monitor variables which share the same port range actually refer to the same monitor, so many references to a single port range for different items and different indexes all use the same monitor internally. In other words, the program avoids creating redundant monitors."
msgstr ""

#: conky.1.xml:3632(emphasis)
