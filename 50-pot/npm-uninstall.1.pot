#: npm-uninstall.1.xml:29(title)
msgid "DESCRIPTION"
msgstr ""

#: npm-uninstall.1.xml:30(para)
msgid "This uninstalls a package, completely removing everything npm installed on its behalf."
msgstr ""

#: npm-uninstall.1.xml:33(para)
msgid "Example:"
msgstr ""

#: npm-uninstall.1.xml:35(literallayout)
#, no-wrap
msgid "\nnpm uninstall sax\n"
msgstr ""

#. .fi
#: npm-uninstall.1.xml:39(para)
msgid "In global mode (ie, with <option>-g</option> or <option>--global</option> appended to the command), it uninstalls the current package context as a global package."
msgstr ""

#: npm-uninstall.1.xml:42(para)
msgid "<emphasis remap=\"B\">npm uninstall</emphasis> takes 3 exclusive, optional flags which save or update the package version in your main package.json:"
msgstr ""

#: npm-uninstall.1.xml:47(para)
msgid "<option>-S, --save</option>: Package will be removed from your <emphasis remap=\"B\">dependencies</emphasis>&thinsp;."
msgstr ""

#: npm-uninstall.1.xml:50(para)
msgid "<option>-D, --save-dev</option>: Package will be removed from your <emphasis remap=\"B\">devDependencies</emphasis>&thinsp;."
msgstr ""

#: npm-uninstall.1.xml:53(para)
msgid "<option>-O, --save-optional</option>: Package will be removed from your <emphasis remap=\"B\">optionalDependencies</emphasis>&thinsp;."
msgstr ""

#. remap='RE'
#: npm-uninstall.1.xml:59(para)
msgid "Further, if you have an <emphasis remap=\"B\">npm-shrinkwrap.json</emphasis> then it will be updated as well."
msgstr ""

#: npm-uninstall.1.xml:62(para)
msgid "Scope is optional and follows the usual rules for npm help 7 <emphasis remap=\"B\">npm-scope</emphasis>&thinsp;."
msgstr ""

#: npm-uninstall.1.xml:64(para)
msgid "Examples:"
msgstr ""

#: npm-uninstall.1.xml:66(literallayout)
#, no-wrap
msgid "\nnpm uninstall sax --save\nnpm uninstall @myorg/privatepackage --save\nnpm uninstall node-tap --save-dev\nnpm uninstall dtrace-provider --save-optional\n"
msgstr ""

