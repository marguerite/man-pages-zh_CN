#. Add any additional description here
#: base64.1.xml:30(para)
msgid "Base64 encode or decode FILE, or standard input, to standard output."
msgstr ""

#: base64.1.xml:32(para)
msgid "Mandatory arguments to long options are mandatory for short options too."
msgstr ""

#: base64.1.xml:35(term)
msgid "<option>-d</option>, <option>--decode</option>"
msgstr ""

#: base64.1.xml:37(para)
msgid "decode data"
msgstr ""

#: base64.1.xml:41(term)
msgid "<option>-i</option>, <option>--ignore-garbage</option>"
msgstr ""

#: base64.1.xml:43(para)
msgid "when decoding, ignore non-alphabet characters"
msgstr ""

#: base64.1.xml:47(term)
msgid "<option>-w</option>, <option>--wrap</option>=<emphasis remap=\"I\">COLS</emphasis>"
msgstr ""

#: base64.1.xml:49(para)
msgid "wrap encoded lines after COLS character (default 76). Use 0 to disable line wrapping"
msgstr ""

#: base64.1.xml:54(option)
msgid "--help"
msgstr ""

#: base64.1.xml:56(para)
msgid "display this help and exit"
msgstr ""

#: base64.1.xml:60(option)
msgid "--version"
msgstr ""

#: base64.1.xml:62(para)
msgid "output version information and exit"
msgstr ""

#: base64.1.xml:64(para)
msgid "With no FILE, or when FILE is -, read standard input."
msgstr ""

#: base64.1.xml:66(para)
msgid "The data are encoded as described for the base64 alphabet in RFC 3548. When decoding, the input may contain newlines in addition to the bytes of the formal base64 alphabet. Use <option>--ignore-garbage</option> to attempt to recover from any other non-alphabet bytes in the encoded stream."
msgstr ""

