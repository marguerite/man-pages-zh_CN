<refpurpose>display a line of text</refpurpose>
</refnamediv>
<!-- body begins here -->
<refsynopsisdiv id='synopsis'>
<cmdsynopsis>
  <command>echo</command>    
    <arg choice='opt' rep='repeat'><replaceable>SHORT-OPTION</replaceable></arg>
    <arg choice='opt' rep='repeat'><replaceable>STRING</replaceable></arg>
    
</cmdsynopsis>
<cmdsynopsis>
  <command>echo</command>    
    <arg choice='plain'><replaceable>LONG-OPTION</replaceable></arg>
</cmdsynopsis>
