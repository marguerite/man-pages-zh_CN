<!-- body begins here -->
<refsynopsisdiv id='synopsis'>
<cmdsynopsis>
  <command>npm</command>    <arg choice='plain'><replaceable>ping</replaceable></arg>
    <arg choice='opt'>--registry <replaceable>&lt;registry&gt;</replaceable></arg>
</cmdsynopsis>
