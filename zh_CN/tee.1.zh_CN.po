#: tee.1.xml:8(refentrytitle)
msgid "TEE"
msgstr "tee"

#: tee.1.xml:9(manvolnum)
msgid "1"
msgstr "1"

#: tee.1.xml:10(refmiscinfo)
msgid "July 2014"
msgstr "2014 年 7 月"

#: tee.1.xml:11(refmiscinfo)
msgid "GNU coreutils 8.22"
msgstr "GNU coreutils 8.22"

#: tee.1.xml:14(refname) tee.1.xml:20(command)
msgid "tee"
msgstr "tee"

#: tee.1.xml:15(refpurpose)
msgid "read from standard input and write to standard output and files"
msgstr "从标准输入读取并写入到标准输出和文件"

#: tee.1.xml:21(replaceable)
msgid "OPTION"
msgstr "选项"

#: tee.1.xml:22(replaceable)
msgid "FILE"
msgstr "FILE"

#: tee.1.xml:28(title)
msgid "DESCRIPTION"
msgstr "描述"

#. Add any additional description here
#: tee.1.xml:31(para)
msgid "Copy standard input to each FILE, and also to standard output."
msgstr "复制标准输入到每个 FILE 和标准输出。"

#: tee.1.xml:34(term)
msgid "<option>-a</option>, <option>--append</option>"
msgstr "<option>-a</option>, <option>--append</option>"

#: tee.1.xml:36(para)
msgid "append to the given FILEs, do not overwrite"
msgstr "追加到给定 FILE 而不是覆盖"

#: tee.1.xml:40(term)
msgid "<option>-i</option>, <option>--ignore-interrupts</option>"
msgstr "<option>-i</option>, <option>--ignore-interrupts</option>"

#: tee.1.xml:42(para)
msgid "ignore interrupt signals"
msgstr "忽略中断信号"

#: tee.1.xml:46(option)
msgid "--help"
msgstr "--help"

#: tee.1.xml:48(para)
msgid "display this help and exit"
msgstr "显示此帮助并退出"

#: tee.1.xml:52(option)
msgid "--version"
msgstr "--version"

#: tee.1.xml:54(para)
msgid "output version information and exit"
msgstr "输出版本信息并退出"

#: tee.1.xml:56(para)
msgid "If a FILE is -, copy again to standard output."
msgstr "若 FILE 是 -，则再次复制到标准输出。"

#: tee.1.xml:58(para)
msgid ""
"GNU coreutils online help: &lt;<ulink url=\"http://www.gnu.org/software/"
"coreutils/&gt;\">http://www.gnu.org/software/coreutils/&gt;</ulink> Report "
"tee translation bugs to &lt;<ulink url=\"http://translationproject.org/team/"
"&gt;\">http://translationproject.org/team/&gt;</ulink>"
msgstr ""
"GNU coreutils 在线帮助：&lt;<ulink url=\"http://www.gnu.org/software/"
"coreutils/&gt;\">http://www.gnu.org/software/coreutils/&gt;</ulink> 汇报 "
"tee 翻译故障 &lt;<ulink url=\"http://translationproject.org/"
"team/&gt;\">http://translationproject.org/team/&gt;</ulink>"

#: tee.1.xml:65(title)
msgid "AUTHOR"
msgstr "作者"

#: tee.1.xml:66(para)
msgid "Written by Mike Parker, Richard M. Stallman, and David MacKenzie."
msgstr "由 Mike Parker、Richard M. Stallman 和 David MacKenzie 编纂。"

