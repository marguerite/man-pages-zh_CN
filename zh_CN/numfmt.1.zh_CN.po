#: numfmt.1.xml:8(refentrytitle)
msgid "NUMFMT"
msgstr ""

#: numfmt.1.xml:9(manvolnum)
msgid "1"
msgstr ""

#: numfmt.1.xml:10(refmiscinfo)
msgid "July 2014"
msgstr ""

#: numfmt.1.xml:11(refmiscinfo)
msgid "GNU coreutils 8.22"
msgstr ""

#: numfmt.1.xml:14(refname) numfmt.1.xml:20(command)
msgid "numfmt"
msgstr ""

#: numfmt.1.xml:15(refpurpose)
msgid "Convert numbers from/to human-readable strings"
msgstr ""

#: numfmt.1.xml:21(replaceable)
msgid "OPTION"
msgstr ""

#: numfmt.1.xml:22(replaceable)
msgid "NUMBER"
msgstr ""

#: numfmt.1.xml:28(title)
msgid "DESCRIPTION"
msgstr ""

#. Add any additional description here
#: numfmt.1.xml:31(para)
msgid ""
"Reformat NUMBER(s), or the numbers from standard input if none are specified."
msgstr ""

#: numfmt.1.xml:33(para)
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr ""

#: numfmt.1.xml:36(option)
msgid "--debug"
msgstr ""

#: numfmt.1.xml:38(para)
msgid "print warnings about invalid input"
msgstr ""

#: numfmt.1.xml:42(term)
msgid ""
"<option>-d</option>, <option>--delimiter</option>=<emphasis remap=\"I\">X</"
"emphasis>"
msgstr ""

#: numfmt.1.xml:44(para)
msgid "use X instead of whitespace for field delimiter"
msgstr ""

#: numfmt.1.xml:48(term)
msgid "<option>--field</option>=<emphasis remap=\"I\">N</emphasis>"
msgstr ""

#: numfmt.1.xml:50(para)
msgid "replace the number in input field N (default is 1)"
msgstr ""

#: numfmt.1.xml:54(term)
msgid "<option>--format</option>=<emphasis remap=\"I\">FORMAT</emphasis>"
msgstr ""

#: numfmt.1.xml:56(para)
msgid "use printf style floating-point FORMAT; see FORMAT below for details"
msgstr ""

#: numfmt.1.xml:61(term)
msgid "<option>--from</option>=<emphasis remap=\"I\">UNIT</emphasis>"
msgstr ""

#: numfmt.1.xml:63(para)
msgid "auto-scale input numbers to UNITs; default is 'none'; see UNIT below"
msgstr ""

#: numfmt.1.xml:68(term)
msgid "<option>--from-unit</option>=<emphasis remap=\"I\">N</emphasis>"
msgstr ""

#: numfmt.1.xml:70(para)
msgid "specify the input unit size (instead of the default 1)"
msgstr ""

#: numfmt.1.xml:74(option)
msgid "--grouping"
msgstr ""

#: numfmt.1.xml:76(para)
msgid ""
"use locale-defined grouping of digits, e.g. 1,000,000 (which means it has no "
"effect in the C/POSIX locale)"
msgstr ""

#: numfmt.1.xml:81(term)
msgid "<option>--header</option>[=<emphasis remap=\"I\">N</emphasis>]"
msgstr ""

#: numfmt.1.xml:83(para)
msgid ""
"print (without converting) the first N header lines; N defaults to 1 if not "
"specified"
msgstr ""

#: numfmt.1.xml:88(term)
msgid "<option>--invalid</option>=<emphasis remap=\"I\">MODE</emphasis>"
msgstr ""

#: numfmt.1.xml:90(para)
msgid ""
"failure mode for invalid numbers: MODE can be: abort (default), fail, warn, "
"ignore"
msgstr ""

#: numfmt.1.xml:95(term)
msgid "<option>--padding</option>=<emphasis remap=\"I\">N</emphasis>"
msgstr ""

#: numfmt.1.xml:97(para)
msgid ""
"pad the output to N characters; positive N will right-align; negative N will "
"left-align; padding is ignored if the output is wider than N; the default is "
"to automatically pad if a whitespace is found"
msgstr ""

#: numfmt.1.xml:105(term)
msgid "<option>--round</option>=<emphasis remap=\"I\">METHOD</emphasis>"
msgstr ""

#: numfmt.1.xml:107(para)
msgid ""
"use METHOD for rounding when scaling; METHOD can be: up, down, from-zero "
"(default), towards-zero, nearest"
msgstr ""

#: numfmt.1.xml:112(term)
msgid "<option>--suffix</option>=<emphasis remap=\"I\">SUFFIX</emphasis>"
msgstr ""

#: numfmt.1.xml:114(para)
msgid ""
"add SUFFIX to output numbers, and accept optional SUFFIX in input numbers"
msgstr ""

#: numfmt.1.xml:119(term)
msgid "<option>--to</option>=<emphasis remap=\"I\">UNIT</emphasis>"
msgstr ""

#: numfmt.1.xml:121(para)
msgid "auto-scale output numbers to UNITs; see UNIT below"
msgstr ""

#: numfmt.1.xml:125(term)
msgid "<option>--to-unit</option>=<emphasis remap=\"I\">N</emphasis>"
msgstr ""

#: numfmt.1.xml:127(para)
msgid "the output unit size (instead of the default 1)"
msgstr ""

#: numfmt.1.xml:131(option)
msgid "--help"
msgstr ""

#: numfmt.1.xml:133(para)
msgid "display this help and exit"
msgstr ""

#: numfmt.1.xml:137(option)
msgid "--version"
msgstr ""

#: numfmt.1.xml:139(para)
msgid "output version information and exit"
msgstr ""

#: numfmt.1.xml:144(title)
msgid "UNIT options:"
msgstr ""

#: numfmt.1.xml:147(term)
msgid "none"
msgstr ""

#: numfmt.1.xml:149(para)
msgid "no auto-scaling is done; suffixes will trigger an error"
msgstr ""

#: numfmt.1.xml:153(term)
msgid "auto"
msgstr ""

#: numfmt.1.xml:155(para)
msgid "accept optional single/two letter suffix:"
msgstr ""

#: numfmt.1.xml:157(para)
msgid "1K = 1000, 1Ki = 1024, 1M = 1000000, 1Mi = 1048576,"
msgstr ""

#: numfmt.1.xml:164(term)
msgid "si"
msgstr ""

#: numfmt.1.xml:166(para) numfmt.1.xml:176(para)
msgid "accept optional single letter suffix:"
msgstr ""

#: numfmt.1.xml:168(para)
msgid "1K = 1000, 1M = 1000000, ..."
msgstr ""

#: numfmt.1.xml:174(term)
msgid "iec"
msgstr ""

#: numfmt.1.xml:178(para)
msgid "1K = 1024, 1M = 1048576, ..."
msgstr ""

#: numfmt.1.xml:184(term)
msgid "iec-i"
msgstr ""

#: numfmt.1.xml:186(para)
msgid "accept optional two-letter suffix:"
msgstr ""

#: numfmt.1.xml:188(para)
msgid "1Ki = 1024, 1Mi = 1048576, ..."
msgstr ""

#: numfmt.1.xml:192(para)
msgid ""
"FORMAT must be suitable for printing one floating-point argument '%f'. "
"Optional quote (%'f) will enable <option>--grouping</option> (if supported "
"by current locale). Optional width value (%10f) will pad output. Optional "
"negative width values (%-10f) will left-pad output."
msgstr ""

#: numfmt.1.xml:197(para)
msgid ""
"Exit status is 0 if all input numbers were successfully converted. By "
"default, numfmt will stop at the first conversion error with exit status 2. "
"With <option>--invalid=</option>'fail' a warning is printed for each "
"conversion error and the exit status is 2. With <option>--invalid=</"
"option>'warn' each conversion error is diagnosed, but the exit status is 0. "
"With <option>--invalid=</option>'ignore' conversion errors are not diagnosed "
"and the exit status is 0."
msgstr ""

#: numfmt.1.xml:209(title)
msgid "EXAMPLES"
msgstr ""

#: numfmt.1.xml:211(emphasis)
msgid "$ numfmt --to=si 1000"
msgstr ""

#: numfmt.1.xml:213(para)
msgid "-&gt; \"1.0K\""
msgstr ""

#: numfmt.1.xml:215(emphasis)
msgid "$ numfmt --to=iec 2048"
msgstr ""

#: numfmt.1.xml:217(para)
msgid "-&gt; \"2.0K\""
msgstr ""

#: numfmt.1.xml:219(emphasis)
msgid "$ numfmt --to=iec-i 4096"
msgstr ""

#: numfmt.1.xml:221(para)
msgid "-&gt; \"4.0Ki\""
msgstr ""

#: numfmt.1.xml:223(emphasis)
msgid "$ echo 1K | numfmt --from=si"
msgstr ""

#: numfmt.1.xml:225(para)
msgid "-&gt; \"1000\""
msgstr ""

#: numfmt.1.xml:227(emphasis)
msgid "$ echo 1K | numfmt --from=iec"
msgstr ""

#: numfmt.1.xml:229(para)
msgid "-&gt; \"1024\""
msgstr ""

#: numfmt.1.xml:231(emphasis)
msgid "$ df | numfmt --header --field 2 --to=si"
msgstr ""

#. br
#: numfmt.1.xml:233(emphasis)
msgid "$ ls -l | numfmt --header --field 5 --to=iec"
msgstr ""

#. br
#: numfmt.1.xml:235(emphasis)
msgid "$ ls -lh | numfmt --header --field 5 --from=iec --padding=10"
msgstr ""

#. br
#: numfmt.1.xml:237(emphasis)
msgid "$ ls -lh | numfmt --header --field 5 --from=iec --format %10f"
msgstr ""

