msgstr ""

#: chgrp.1.xml:14(refname) chgrp.1.xml:20(command) chgrp.1.xml:27(command)
msgid "chgrp"
msgstr ""

#: chgrp.1.xml:15(refpurpose)
msgid "change group ownership"
msgstr ""

#: chgrp.1.xml:21(replaceable) chgrp.1.xml:28(replaceable)
msgid "OPTION"
msgstr ""

#: chgrp.1.xml:22(replaceable)
msgid "GROUP"
msgstr ""

#: chgrp.1.xml:23(replaceable) chgrp.1.xml:30(replaceable)
msgid "FILE"
msgstr ""

#: chgrp.1.xml:29(replaceable)
msgid "RFILE"
msgstr ""

#: chgrp.1.xml:29(arg)
msgid "--reference=<placeholder-1/>"
msgstr ""

#: chgrp.1.xml:36(title)
msgid "DESCRIPTION"
msgstr ""

#. Add any additional description here
#: chgrp.1.xml:39(para)
msgid ""
"Change the group of each FILE to GROUP. With <option>--reference</option>, "
"change the group of each FILE to that of RFILE."
msgstr ""

#: chgrp.1.xml:43(term)
msgid "<option>-c</option>, <option>--changes</option>"
msgstr ""

#: chgrp.1.xml:45(para)
msgid "like verbose but report only when a change is made"
msgstr ""

#: chgrp.1.xml:49(term)
msgid ""
"<option>-f</option>, <option>--silent</option>, <option>--quiet</option>"
msgstr ""

#: chgrp.1.xml:51(para)
msgid "suppress most error messages"
msgstr ""

#: chgrp.1.xml:55(term)
msgid "<option>-v</option>, <option>--verbose</option>"
msgstr ""

#: chgrp.1.xml:57(para)
msgid "output a diagnostic for every file processed"
msgstr ""

#: chgrp.1.xml:61(option)
msgid "--dereference"
msgstr ""

#: chgrp.1.xml:63(para)
msgid ""
"affect the referent of each symbolic link (this is the default), rather than "
"the symbolic link itself"
msgstr ""

#: chgrp.1.xml:68(term)
msgid "<option>-h</option>, <option>--no-dereference</option>"
msgstr ""

#: chgrp.1.xml:70(para)
msgid ""
"affect symbolic links instead of any referenced file (useful only on systems "
"that can change the ownership of a symlink)"
msgstr ""

#: chgrp.1.xml:76(option)
msgid "--no-preserve-root"
msgstr ""

#: chgrp.1.xml:78(para)
msgid "do not treat '/' specially (the default)"
msgstr ""

#: chgrp.1.xml:82(option)
msgid "--preserve-root"
msgstr ""

#: chgrp.1.xml:84(para)
msgid "fail to operate recursively on '/'"
msgstr ""

#: chgrp.1.xml:88(term)
msgid "<option>--reference</option>=<emphasis remap=\"I\">RFILE</emphasis>"
msgstr ""

#: chgrp.1.xml:90(para)
msgid "use RFILE's group rather than specifying a GROUP value"
msgstr ""

#: chgrp.1.xml:95(term)
msgid "<option>-R</option>, <option>--recursive</option>"
msgstr ""

#: chgrp.1.xml:97(para)
msgid "operate on files and directories recursively"
msgstr ""

#: chgrp.1.xml:99(para)
msgid ""
"The following options modify how a hierarchy is traversed when the <option>-"
"R</option> option is also specified. If more than one is specified, only the "
"final one takes effect."
msgstr ""

#: chgrp.1.xml:105(option)
msgid "-H"
msgstr ""

#: chgrp.1.xml:107(para)
msgid ""
"if a command line argument is a symbolic link to a directory, traverse it"
msgstr ""

#: chgrp.1.xml:112(option)
msgid "-L"
msgstr ""

#: chgrp.1.xml:114(para)
msgid "traverse every symbolic link to a directory encountered"
msgstr ""

#: chgrp.1.xml:119(option)
msgid "-P"
msgstr ""

#: chgrp.1.xml:121(para)
msgid "do not traverse any symbolic links (default)"
msgstr ""

#: chgrp.1.xml:125(option)
msgid "--help"
msgstr ""

#: chgrp.1.xml:127(para)
msgid "display this help and exit"
msgstr ""

#: chgrp.1.xml:131(option)
msgid "--version"
msgstr ""

#: chgrp.1.xml:133(para)
msgid "output version information and exit"
msgstr ""

#: chgrp.1.xml:139(title)
msgid "EXAMPLES"
msgstr ""

#: chgrp.1.xml:142(term)
msgid "chgrp staff /u"
msgstr ""

#: chgrp.1.xml:144(para)
msgid "Change the group of /u to \"staff\"."
msgstr ""

#: chgrp.1.xml:148(term)
msgid "chgrp -hR staff /u"
msgstr ""

#: chgrp.1.xml:150(para)
msgid "Change the group of /u and subfiles to \"staff\"."
msgstr ""

