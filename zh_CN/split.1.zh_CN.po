#: split.1.xml:27(title)
msgid "DESCRIPTION"
msgstr ""

#. Add any additional description here
#: split.1.xml:30(para)
msgid ""
"Output fixed-size pieces of INPUT to PREFIXaa, PREFIXab, ...; default size "
"is 1000 lines, and default PREFIX is 'x'. With no INPUT, or when INPUT is -, "
"read standard input."
msgstr ""

#: split.1.xml:34(para)
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr ""

#: split.1.xml:37(term)
msgid ""
"<option>-a</option>, <option>--suffix-length</option>=<emphasis remap=\"I"
"\">N</emphasis>"
msgstr ""

#: split.1.xml:39(para)
msgid "generate suffixes of length N (default 2)"
msgstr ""

#: split.1.xml:43(term)
msgid ""
"<option>--additional-suffix</option>=<emphasis remap=\"I\">SUFFIX</emphasis>"
msgstr ""

#: split.1.xml:45(para)
msgid "append an additional SUFFIX to file names"
msgstr ""

#: split.1.xml:49(term)
msgid ""
"<option>-b</option>, <option>--bytes</option>=<emphasis remap=\"I\">SIZE</"
"emphasis>"
msgstr ""

#: split.1.xml:51(para)
msgid "put SIZE bytes per output file"
msgstr ""

#: split.1.xml:55(term)
msgid ""
"<option>-C</option>, <option>--line-bytes</option>=<emphasis remap=\"I"
"\">SIZE</emphasis>"
msgstr ""

#: split.1.xml:57(para)
msgid "put at most SIZE bytes of lines per output file"
msgstr ""

#: split.1.xml:61(term)
msgid ""
"<option>-d</option>, <option>--numeric-suffixes</option>[=<emphasis remap=\"I"
"\">FROM</emphasis>]"
msgstr ""

#: split.1.xml:63(para)
msgid ""
"use numeric suffixes instead of alphabetic; FROM changes the start value "
"(default 0)"
msgstr ""

#: split.1.xml:68(term)
msgid "<option>-e</option>, <option>--elide-empty-files</option>"
msgstr ""

#: split.1.xml:70(para)
msgid "do not generate empty output files with '-n'"
msgstr ""

#: split.1.xml:74(term)
msgid "<option>--filter</option>=<emphasis remap=\"I\">COMMAND</emphasis>"
msgstr ""

#: split.1.xml:76(para)
msgid "write to shell COMMAND; file name is $FILE"
msgstr ""

#: split.1.xml:80(term)
msgid ""
"<option>-l</option>, <option>--lines</option>=<emphasis remap=\"I\">NUMBER</"
"emphasis>"
msgstr ""

#: split.1.xml:82(para)
msgid "put NUMBER lines per output file"
msgstr ""

#: split.1.xml:86(term)
msgid ""
"<option>-n</option>, <option>--number</option>=<emphasis remap=\"I\">CHUNKS</"
"emphasis>"
msgstr ""

#: split.1.xml:88(para)
msgid "generate CHUNKS output files; see explanation below"
msgstr ""

#: split.1.xml:92(term)
msgid "<option>-u</option>, <option>--unbuffered</option>"
msgstr ""

#: split.1.xml:94(para)
msgid "immediately copy input to output with '-n r/...'"
msgstr ""

#: split.1.xml:98(option)
msgid "--verbose"
msgstr ""

#: split.1.xml:100(para)
msgid "print a diagnostic just before each output file is opened"
msgstr ""

#: split.1.xml:105(option)
msgid "--help"
msgstr ""

#: split.1.xml:107(para)
msgid "display this help and exit"
msgstr ""

#: split.1.xml:111(option)
msgid "--version"
msgstr ""

#: split.1.xml:113(para)
msgid "output version information and exit"
msgstr ""

#: split.1.xml:115(para)
msgid ""
"The SIZE argument is an integer and optional unit (example: 10K is 10*1024). "
"Units are K,M,G,T,P,E,Z,Y (powers of 1024) or KB,MB,... (powers of 1000)."
msgstr ""

#: split.1.xml:118(para)
msgid ""
"CHUNKS may be: N split into N files based on size of input K/N output Kth of "
"N to stdout l/N split into N files without splitting lines l/K/N output Kth "
"of N to stdout without splitting lines r/N like 'l' but use round robin "
"distribution r/K/N likewise but only output Kth of N to stdout"
msgstr ""

