#: ibus-daemon.1.xml:13(manvolnum) ibus-daemon.1.xml:151(manvolnum)
msgid "1"
msgstr "1"

#: ibus-daemon.1.xml:14(refmiscinfo)
msgid "April 2010"
msgstr "2010 年 4 月"

#: ibus-daemon.1.xml:15(refmiscinfo)
msgid "1.5.8"
msgstr "1.5.8"

