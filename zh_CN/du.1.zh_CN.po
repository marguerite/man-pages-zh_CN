#: du.1.xml:8(refentrytitle)
msgid "DU"
msgstr ""

#: du.1.xml:9(manvolnum)
msgid "1"
msgstr ""

#: du.1.xml:10(refmiscinfo)
msgid "July 2014"
msgstr ""

#: du.1.xml:11(refmiscinfo)
msgid "GNU coreutils 8.22"
msgstr ""

#: du.1.xml:14(refname) du.1.xml:20(command) du.1.xml:26(command)
msgid "du"
msgstr ""

#: du.1.xml:15(refpurpose)
msgid "estimate file space usage"
msgstr ""

#: du.1.xml:21(replaceable) du.1.xml:27(replaceable)
msgid "OPTION"
msgstr ""

#: du.1.xml:22(replaceable)
msgid "FILE"
msgstr ""

#: du.1.xml:28(replaceable)
msgid "F"
msgstr ""

#: du.1.xml:28(arg)
msgid "--files0-from=<placeholder-1/>"
msgstr ""

#: du.1.xml:33(title)
msgid "DESCRIPTION"
msgstr ""

#. Add any additional description here
#: du.1.xml:36(para)
msgid "Summarize disk usage of each FILE, recursively for directories."
msgstr ""

#: du.1.xml:38(para)
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr ""

#: du.1.xml:41(term)
msgid "<option>-0</option>, <option>--null</option>"
msgstr ""

#: du.1.xml:43(para)
msgid "end each output line with 0 byte rather than newline"
msgstr ""

#: du.1.xml:47(term)
msgid "<option>-a</option>, <option>--all</option>"
msgstr ""

#: du.1.xml:49(para)
msgid "write counts for all files, not just directories"
msgstr ""

#: du.1.xml:53(option)
msgid "--apparent-size"
msgstr ""

#: du.1.xml:55(para)
msgid ""
"print apparent sizes, rather than disk usage; although the apparent size is "
"usually smaller, it may be larger due to holes in ('sparse') files, internal "
"fragmentation, indirect blocks, and the like"
msgstr ""

#: du.1.xml:62(term)
msgid ""
"<option>-B</option>, <option>--block-size</option>=<emphasis remap=\"I"
"\">SIZE</emphasis>"
msgstr ""

#: du.1.xml:64(para)
msgid ""
"scale sizes by SIZE before printing them; e.g., '-BM' prints sizes in units "
"of 1,048,576 bytes; see SIZE format below"
msgstr ""

#: du.1.xml:70(term)
msgid "<option>-b</option>, <option>--bytes</option>"
msgstr ""

#: du.1.xml:72(para)
msgid ""
"equivalent to '--apparent-size <option>--block-size</option>=<literal>1</"
"literal>'"
msgstr ""

#: du.1.xml:76(term)
msgid "<option>-c</option>, <option>--total</option>"
msgstr ""

#: du.1.xml:78(para)
msgid "produce a grand total"
msgstr ""

#: du.1.xml:82(term)
msgid "<option>-D</option>, <option>--dereference-args</option>"
msgstr ""

#: du.1.xml:84(para)
msgid "dereference only symlinks that are listed on the command line"
msgstr ""

#: du.1.xml:89(term)
msgid ""
"<option>-d</option>, <option>--max-depth</option>=<emphasis remap=\"I\">N</"
"emphasis>"
msgstr ""

#: du.1.xml:91(para)
msgid ""
"print the total for a directory (or file, with <option>--all</option>) only "
"if it is N or fewer levels below the command line argument; <option>--max-"
"depth</option>=<literal>0</literal> is the same as <option>--summarize</"
"option>"
msgstr ""

#: du.1.xml:98(term)
msgid "<option>--files0-from</option>=<emphasis remap=\"I\">F</emphasis>"
msgstr ""

#: du.1.xml:100(para)
msgid ""
"summarize disk usage of the NUL-terminated file names specified in file F; "
"if F is -, then read names from standard input"
msgstr ""

#: du.1.xml:106(option)
msgid "-H"
msgstr ""

#: du.1.xml:108(para)
msgid "equivalent to <option>--dereference-args</option> (<option>-D</option>)"
msgstr ""

#: du.1.xml:112(term)
msgid "<option>-h</option>, <option>--human-readable</option>"
msgstr ""

#: du.1.xml:114(para)
msgid "print sizes in human readable format (e.g., 1K 234M 2G)"
msgstr ""

#: du.1.xml:118(option)
msgid "--inodes"
msgstr ""

#: du.1.xml:120(para)
msgid "list inode usage information instead of block usage"
msgstr ""

#: du.1.xml:124(option)
msgid "-k"
msgstr ""

#: du.1.xml:126(para)
msgid "like <option>--block-size</option>=<emphasis remap=\"I\">1K</emphasis>"
msgstr ""

#: du.1.xml:130(term)
msgid "<option>-L</option>, <option>--dereference</option>"
msgstr ""

#: du.1.xml:132(para)
msgid "dereference all symbolic links"
msgstr ""

#: du.1.xml:136(term)
msgid "<option>-l</option>, <option>--count-links</option>"
msgstr ""

#: du.1.xml:138(para)
msgid "count sizes many times if hard linked"
msgstr ""

#: du.1.xml:142(option)
msgid "-m"
msgstr ""

#: du.1.xml:144(para)
msgid "like <option>--block-size</option>=<emphasis remap=\"I\">1M</emphasis>"
msgstr ""

#: du.1.xml:148(term)
msgid "<option>-P</option>, <option>--no-dereference</option>"
msgstr ""

#: du.1.xml:150(para)
msgid "don't follow any symbolic links (this is the default)"
msgstr ""

#: du.1.xml:154(term)
msgid "<option>-S</option>, <option>--separate-dirs</option>"
msgstr ""

#: du.1.xml:156(para)
msgid "for directories do not include size of subdirectories"
msgstr ""

#: du.1.xml:160(option)
msgid "--si"
msgstr ""

#: du.1.xml:162(para)
msgid "like <option>-h</option>, but use powers of 1000 not 1024"
msgstr ""

#: du.1.xml:166(term)
msgid "<option>-s</option>, <option>--summarize</option>"
msgstr ""

#: du.1.xml:168(para)
msgid "display only a total for each argument"
msgstr ""

#: du.1.xml:172(term)
msgid ""
"<option>-t</option>, <option>--threshold</option>=<emphasis remap=\"I"
"\">SIZE</emphasis>"
msgstr ""

#: du.1.xml:174(para)
msgid ""
"exclude entries smaller than SIZE if positive, or entries greater than SIZE "
"if negative"
msgstr ""

#: du.1.xml:179(option)
msgid "--time"
msgstr ""

#: du.1.xml:181(para)
msgid ""
"show time of the last modification of any file in the directory, or any of "
"its subdirectories"
msgstr ""

#: du.1.xml:186(term)
msgid "<option>--time</option>=<emphasis remap=\"I\">WORD</emphasis>"
msgstr ""

#: du.1.xml:188(para)
msgid ""
"show time as WORD instead of modification time: atime, access, use, ctime or "
"status"
msgstr ""

#: du.1.xml:193(term)
msgid "<option>--time-style</option>=<emphasis remap=\"I\">STYLE</emphasis>"
msgstr ""

#: du.1.xml:195(para)
msgid ""
"show times using STYLE, which can be: full-iso, long-iso, iso, or +FORMAT; "
"FORMAT is interpreted like in 'date'"
msgstr ""

#: du.1.xml:201(term)
msgid ""
"<option>-X</option>, <option>--exclude-from</option>=<emphasis remap=\"I"
"\">FILE</emphasis>"
msgstr ""

#: du.1.xml:203(para)
msgid "exclude files that match any pattern in FILE"
msgstr ""

#: du.1.xml:207(term)
msgid "<option>--exclude</option>=<emphasis remap=\"I\">PATTERN</emphasis>"
msgstr ""

#: du.1.xml:209(para)
msgid "exclude files that match PATTERN"
msgstr ""

#: du.1.xml:213(term)
msgid "<option>-x</option>, <option>--one-file-system</option>"
msgstr ""

#: du.1.xml:215(para)
msgid "skip directories on different file systems"
msgstr ""

#: du.1.xml:219(option)
msgid "--help"
msgstr ""

#: du.1.xml:221(para)
msgid "display this help and exit"
msgstr ""

#: du.1.xml:225(option)
msgid "--version"
msgstr ""

#: du.1.xml:227(para)
msgid "output version information and exit"
msgstr ""

#: du.1.xml:229(para)
msgid ""
"Display values are in units of the first available SIZE from <option>--block-"
"size</option>, and the DU_BLOCK_SIZE, BLOCK_SIZE and BLOCKSIZE environment "
"variables. Otherwise, units default to 1024 bytes (or 512 if POSIXLY_CORRECT "
"is set)."
msgstr ""

#: du.1.xml:233(para)
msgid ""
"The SIZE argument is an integer and optional unit (example: 10K is 10*1024). "
"Units are K,M,G,T,P,E,Z,Y (powers of 1024) or KB,MB,... (powers of 1000)."
msgstr ""

#: du.1.xml:236(para)
msgid ""
"GNU coreutils online help: &lt;<ulink url=\"http://www.gnu.org/software/"
"coreutils/&gt;\">http://www.gnu.org/software/coreutils/&gt;</ulink> Report "
"du translation bugs to &lt;<ulink url=\"http://translationproject.org/team/"
"&gt;\">http://translationproject.org/team/&gt;</ulink>"
msgstr ""

#: du.1.xml:243(title)
msgid "PATTERNS"
msgstr ""

#: du.1.xml:244(para)
msgid ""
"PATTERN is a shell pattern (not a regular expression). The pattern <emphasis "
"remap=\"B\">?</emphasis> matches any one character, whereas <emphasis remap="
"\"B\">*</emphasis> matches any string (composed of zero, one or multiple "
"characters). For example, <emphasis remap=\"B\">*.o</emphasis> will match "
"any files whose names end in <markup>.o</markup>. Therefore, the command"
msgstr ""

#: du.1.xml:255(userinput)
#, no-wrap
msgid "du --exclude=''*.o''"
msgstr ""

#: du.1.xml:257(para)
msgid ""
"will skip all files and subdirectories ending in <markup>.o</markup> "
"(including the file <markup>.o</markup> itself)."
msgstr ""

