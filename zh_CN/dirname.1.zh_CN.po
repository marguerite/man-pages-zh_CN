#: dirname.1.xml:8(refentrytitle)
msgid "DIRNAME"
msgstr "dirname"

#: dirname.1.xml:9(manvolnum)
msgid "1"
msgstr "1"

#: dirname.1.xml:10(refmiscinfo)
msgid "July 2014"
msgstr "2014 年 7 月"

#: dirname.1.xml:11(refmiscinfo)
msgid "GNU coreutils 8.22"
msgstr "GNU coreutils 8.22"

#: dirname.1.xml:14(refname) dirname.1.xml:20(command)
msgid "dirname"
msgstr "dirname"

#: dirname.1.xml:15(refpurpose)
msgid "strip last component from file name"
msgstr "剔除文件名的最后部分"

#: dirname.1.xml:21(replaceable)
msgid "OPTION"
msgstr "选项"

#: dirname.1.xml:22(replaceable)
msgid "NAME"
msgstr "名称"

#: dirname.1.xml:28(title)
msgid "DESCRIPTION"
msgstr "描述"

#. Add any additional description here
#: dirname.1.xml:31(para)
msgid ""
"Output each NAME with its last non-slash component and trailing slashes "
"removed; if NAME contains no /'s, output '.' (meaning the current directory)."
msgstr ""

#: dirname.1.xml:35(term)
msgid "<option>-z</option>, <option>--zero</option>"
msgstr "<option>-z</option>, <option>--zero</option>"

#: dirname.1.xml:37(para)
msgid "separate output with NUL rather than newline"
msgstr "输出以 NUL 而不是新行分隔"

#: dirname.1.xml:41(option)
msgid "--help"
msgstr "--help"

#: dirname.1.xml:43(para)
msgid "display this help and exit"
msgstr "显示此帮助并退出"

#: dirname.1.xml:47(option)
msgid "--version"
msgstr "--version"

#: dirname.1.xml:49(para)
msgid "output version information and exit"
msgstr "输出版本信息并退出"

#: dirname.1.xml:55(title)
msgid "EXAMPLES"
msgstr "范例"

#: dirname.1.xml:58(term)
msgid "dirname /usr/bin/"
msgstr "dirname /usr/bin/"

#: dirname.1.xml:60(para)
msgid "-&gt; \"/usr\""
msgstr "-&gt; \"/usr\""

#: dirname.1.xml:64(term)
msgid "dirname dir1/str dir2/str"
msgstr "dirname dir1/str dir2/str"

#: dirname.1.xml:66(para)
msgid "-&gt; \"dir1\" followed by \"dir2\""
msgstr "-&gt; \"dir1\" 跟着下一行是 \"dir2\""

#: dirname.1.xml:70(term)
msgid "dirname stdio.h"
msgstr "dirname stdio.h"

#: dirname.1.xml:72(para)
msgid "-&gt; \".\""
msgstr "-&gt; \".\""

