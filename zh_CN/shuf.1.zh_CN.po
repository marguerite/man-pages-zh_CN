#: shuf.1.xml:40(title)
msgid "DESCRIPTION"
msgstr ""

#. Add any additional description here
#: shuf.1.xml:43(para)
msgid "Write a random permutation of the input lines to standard output."
msgstr ""

#: shuf.1.xml:45(para)
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr ""

#: shuf.1.xml:48(term)
msgid "<option>-e</option>, <option>--echo</option>"
msgstr ""

#: shuf.1.xml:50(para)
msgid "treat each ARG as an input line"
msgstr ""

#: shuf.1.xml:54(term)
msgid ""
"<option>-i</option>, <option>--input-range</option>=<emphasis remap=\"I\">LO-"
"HI</emphasis>"
msgstr ""

#: shuf.1.xml:56(para)
msgid "treat each number LO through HI as an input line"
msgstr ""

#: shuf.1.xml:60(term)
msgid ""
"<option>-n</option>, <option>--head-count</option>=<emphasis remap=\"I"
"\">COUNT</emphasis>"
msgstr ""

#: shuf.1.xml:62(para)
msgid "output at most COUNT lines"
msgstr ""

#: shuf.1.xml:66(term)
msgid ""
"<option>-o</option>, <option>--output</option>=<emphasis remap=\"I\">FILE</"
"emphasis>"
msgstr ""

#: shuf.1.xml:68(para)
msgid "write result to FILE instead of standard output"
msgstr ""

#: shuf.1.xml:72(term)
msgid "<option>--random-source</option>=<emphasis remap=\"I\">FILE</emphasis>"
msgstr ""

#: shuf.1.xml:74(para)
msgid "get random bytes from FILE"
msgstr ""

#: shuf.1.xml:78(term)
msgid "<option>-r</option>, <option>--repeat</option>"
msgstr ""

#: shuf.1.xml:80(para)
msgid "output lines can be repeated"
msgstr ""

#: shuf.1.xml:84(term)
msgid "<option>-z</option>, <option>--zero-terminated</option>"
msgstr ""

#: shuf.1.xml:86(para)
msgid "end lines with 0 byte, not newline"
msgstr ""

#: shuf.1.xml:90(option)
msgid "--help"
msgstr ""

#: shuf.1.xml:92(para)
msgid "display this help and exit"
msgstr ""

#: shuf.1.xml:96(option)
msgid "--version"
msgstr ""

#: shuf.1.xml:98(para)
msgid "output version information and exit"
msgstr ""

#: shuf.1.xml:100(para)
msgid "With no FILE, or when FILE is -, read standard input."
msgstr ""

