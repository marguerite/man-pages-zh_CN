#: cp.1.xml:14(refname) cp.1.xml:20(command) cp.1.xml:27(command)
#: cp.1.xml:33(command)
msgid "cp"
msgstr ""

#: cp.1.xml:15(refpurpose)
msgid "copy files and directories"
msgstr ""

#: cp.1.xml:21(replaceable) cp.1.xml:28(replaceable) cp.1.xml:34(replaceable)
msgid "OPTION"
msgstr ""

#: cp.1.xml:22(arg)
msgid "-T"
msgstr ""

#: cp.1.xml:23(replaceable) cp.1.xml:29(replaceable) cp.1.xml:36(replaceable)
msgid "SOURCE"
msgstr ""

#: cp.1.xml:24(replaceable)
msgid "DEST"
msgstr ""

#: cp.1.xml:30(replaceable) cp.1.xml:35(replaceable)
msgid "DIRECTORY"
msgstr ""

#: cp.1.xml:35(arg)
msgid "-t <placeholder-1/>"
msgstr ""

#: cp.1.xml:42(title)
msgid "DESCRIPTION"
msgstr ""

#. Add any additional description here
#: cp.1.xml:45(para)
msgid "Copy SOURCE to DEST, or multiple SOURCE(s) to DIRECTORY."
msgstr ""

#: cp.1.xml:47(para)
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr ""

#: cp.1.xml:50(term)
msgid "<option>-a</option>, <option>--archive</option>"
msgstr ""

#: cp.1.xml:52(para)
msgid ""
"same as <option>-dR</option><option>--preserve</option>=<emphasis remap=\"I"
"\">all</emphasis>"
msgstr ""

#: cp.1.xml:56(option)
msgid "--attributes-only"
msgstr ""

#: cp.1.xml:58(para)
msgid "don't copy the file data, just the attributes"
msgstr ""

#: cp.1.xml:62(term)
msgid "<option>--backup</option>[=<emphasis remap=\"I\">CONTROL</emphasis>]"
msgstr ""

#: cp.1.xml:64(para)
msgid "make a backup of each existing destination file"
msgstr ""

#: cp.1.xml:68(option)
msgid "-b"
msgstr ""

#: cp.1.xml:70(para)
msgid "like <option>--backup</option> but does not accept an argument"
msgstr ""

#: cp.1.xml:74(option)
msgid "--copy-contents"
msgstr ""

#: cp.1.xml:76(para)
msgid "copy contents of special files when recursive"
msgstr ""

#: cp.1.xml:80(option)
msgid "-d"
msgstr ""

#: cp.1.xml:82(para)
msgid ""
"same as <option>--no-dereference</option><option>--preserve</"
"option>=<emphasis remap=\"I\">links</emphasis>"
msgstr ""

#: cp.1.xml:86(term)
msgid "<option>-f</option>, <option>--force</option>"
msgstr ""

#: cp.1.xml:88(para)
msgid ""
"if an existing destination file cannot be opened, remove it and try again "
"(this option is ignored when the <option>-n</option> option is also used)"
msgstr ""

#: cp.1.xml:94(term)
msgid "<option>-i</option>, <option>--interactive</option>"
msgstr ""

#: cp.1.xml:96(para)
msgid ""
"prompt before overwrite (overrides a previous <option>-n</option> option)"
msgstr ""

#: cp.1.xml:101(option)
msgid "-H"
msgstr ""

#: cp.1.xml:103(para)
msgid "follow command-line symbolic links in SOURCE"
msgstr ""

#: cp.1.xml:107(term)
msgid "<option>-l</option>, <option>--link</option>"
msgstr ""

#: cp.1.xml:109(para)
msgid "hard link files instead of copying"
msgstr ""

#: cp.1.xml:113(term)
msgid "<option>-L</option>, <option>--dereference</option>"
msgstr ""

#: cp.1.xml:115(para)
msgid "always follow symbolic links in SOURCE"
msgstr ""

#: cp.1.xml:119(term)
msgid "<option>-n</option>, <option>--no-clobber</option>"
msgstr ""

#: cp.1.xml:121(para)
msgid ""
"do not overwrite an existing file (overrides a previous <option>-i</option> "
"option)"
msgstr ""

#: cp.1.xml:126(term)
msgid "<option>-P</option>, <option>--no-dereference</option>"
msgstr ""

#: cp.1.xml:128(para)
msgid "never follow symbolic links in SOURCE"
msgstr ""

#: cp.1.xml:132(option)
msgid "-p"
msgstr ""

#: cp.1.xml:134(para)
msgid ""
"same as <option>--preserve</option>=<emphasis remap=\"I\">mode</emphasis>,"
"ownership,timestamps"
msgstr ""

#: cp.1.xml:138(term)
msgid ""
"<option>--preserve</option>[=<emphasis remap=\"I\">ATTR_LIST</emphasis>]"
msgstr ""

#: cp.1.xml:140(para)
msgid ""
"preserve the specified attributes (default: mode,ownership,timestamps), if "
"possible additional attributes: context, links, xattr, all"
msgstr ""

#: cp.1.xml:147(term)
msgid ""
"<option>--no-preserve</option>=<emphasis remap=\"I\">ATTR_LIST</emphasis>"
msgstr ""

#: cp.1.xml:149(para)
msgid "don't preserve the specified attributes"
msgstr ""

#: cp.1.xml:153(option)
msgid "--parents"
msgstr ""

#: cp.1.xml:155(para)
msgid "use full source file name under DIRECTORY"
msgstr ""

#: cp.1.xml:159(term)
msgid "<option>-R</option>, <option>-r</option>, <option>--recursive</option>"
msgstr ""

#: cp.1.xml:161(para)
msgid "copy directories recursively"
msgstr ""

#: cp.1.xml:165(term)
msgid "<option>--reflink</option>[=<emphasis remap=\"I\">WHEN</emphasis>]"
msgstr ""

#: cp.1.xml:167(para)
msgid "control clone/CoW copies. See below"
msgstr ""

#: cp.1.xml:171(option)
msgid "--remove-destination"
msgstr ""

#: cp.1.xml:173(para)
msgid ""
"remove each existing destination file before attempting to open it (contrast "
"with <option>--force</option>)"
msgstr ""

#: cp.1.xml:178(term)
msgid "<option>--sparse</option>=<emphasis remap=\"I\">WHEN</emphasis>"
msgstr ""

#: cp.1.xml:180(para)
msgid "control creation of sparse files. See below"
msgstr ""

#: cp.1.xml:184(option)
msgid "--strip-trailing-slashes"
msgstr ""

#: cp.1.xml:186(para)
msgid "remove any trailing slashes from each SOURCE argument"
msgstr ""

#: cp.1.xml:191(term)
msgid "<option>-s</option>, <option>--symbolic-link</option>"
msgstr ""

#: cp.1.xml:193(para)
msgid "make symbolic links instead of copying"
msgstr ""

#: cp.1.xml:197(term)
msgid ""
"<option>-S</option>, <option>--suffix</option>=<emphasis remap=\"I\">SUFFIX</"
"emphasis>"
msgstr ""

#: cp.1.xml:199(para)
msgid "override the usual backup suffix"
msgstr ""

#: cp.1.xml:203(term)
msgid ""
"<option>-t</option>, <option>--target-directory</option>=<emphasis remap=\"I"
"\">DIRECTORY</emphasis>"
msgstr ""

#: cp.1.xml:205(para)
msgid "copy all SOURCE arguments into DIRECTORY"
msgstr ""

#: cp.1.xml:209(term)
msgid "<option>-T</option>, <option>--no-target-directory</option>"
msgstr ""

#: cp.1.xml:211(para)
msgid "treat DEST as a normal file"
msgstr ""

#: cp.1.xml:215(term)
msgid "<option>-u</option>, <option>--update</option>"
msgstr ""

#: cp.1.xml:217(para)
msgid ""
"copy only when the SOURCE file is newer than the destination file or when "
"the destination file is missing"
msgstr ""

#: cp.1.xml:223(term)
msgid "<option>-v</option>, <option>--verbose</option>"
msgstr ""

#: cp.1.xml:225(para)
msgid "explain what is being done"
msgstr ""

#: cp.1.xml:229(term)
msgid "<option>-x</option>, <option>--one-file-system</option>"
msgstr ""

#: cp.1.xml:231(para)
msgid "stay on this file system"
msgstr ""

#: cp.1.xml:235(term)
msgid ""
"<option>-Z</option>, <option>--context</option>[=<emphasis remap=\"I\">CTX</"
"emphasis>]"
msgstr ""

#: cp.1.xml:237(para)
msgid ""
"set SELinux security context of destination file to default type, or to CTX "
"if specified"
msgstr ""

#: cp.1.xml:242(option)
msgid "--help"
msgstr ""

#: cp.1.xml:244(para)
msgid "display this help and exit"
msgstr ""

#: cp.1.xml:248(option)
msgid "--version"
msgstr ""

#: cp.1.xml:250(para)
msgid "output version information and exit"
msgstr ""

#: cp.1.xml:252(para)
msgid ""
"By default, sparse SOURCE files are detected by a crude heuristic and the "
"corresponding DEST file is made sparse as well. That is the behavior "
"selected by <option>--sparse</option>=<emphasis remap=\"I\">auto</emphasis>. "
"Specify <option>--sparse</option>=<emphasis remap=\"I\">always</emphasis> to "
"create a sparse DEST file whenever the SOURCE file contains a long enough "
"sequence of zero bytes. Use <option>--sparse</option>=<emphasis remap=\"I"
"\">never</emphasis> to inhibit creation of sparse files."
msgstr ""

#: cp.1.xml:258(para)
msgid ""
"When <option>--reflink</option>[=<emphasis remap=\"I\">always</emphasis>] is "
"specified, perform a lightweight copy, where the data blocks are copied only "
"when modified. If this is not possible the copy fails, or if <option>--"
"reflink</option>=<emphasis remap=\"I\">auto</emphasis> is specified, fall "
"back to a standard copy."
msgstr ""

#: cp.1.xml:262(para)
msgid ""
"The backup suffix is '~', unless set with <option>--suffix</option> or "
"SIMPLE_BACKUP_SUFFIX. The version control method may be selected via the "
"<option>--backup</option> option or through the VERSION_CONTROL environment "
"variable. Here are the values:"
msgstr ""

#: cp.1.xml:268(term)
msgid "none, off"
msgstr ""

#: cp.1.xml:270(para)
msgid "never make backups (even if <option>--backup</option> is given)"
msgstr ""

#: cp.1.xml:274(term)
msgid "numbered, t"
msgstr ""

#: cp.1.xml:276(para)
msgid "make numbered backups"
msgstr ""

#: cp.1.xml:280(term)
msgid "existing, nil"
msgstr ""

#: cp.1.xml:282(para)
msgid "numbered if numbered backups exist, simple otherwise"
msgstr ""

#: cp.1.xml:286(term)
msgid "simple, never"
msgstr ""

#: cp.1.xml:288(para)
msgid "always make simple backups"
msgstr ""

#: cp.1.xml:290(para)
msgid ""
"As a special case, cp makes a backup of SOURCE when the force and backup "
"options are given and SOURCE and DEST are the same name for an existing, "
"regular file."
msgstr ""

#: cp.1.xml:294(para)
msgid ""
