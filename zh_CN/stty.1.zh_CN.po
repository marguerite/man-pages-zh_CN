msgid "* [-]olcuc"
msgstr ""

#: stty.1.xml:465(para)
msgid "translate lowercase characters to uppercase"
msgstr ""

#: stty.1.xml:469(term)
msgid "* [-]onlcr"
msgstr ""

#: stty.1.xml:471(para)
