#: stty.1.xml:445(term)
msgid "* [-]ocrnl"
msgstr ""

#: stty.1.xml:451(term)
msgid "* [-]ofdel"
msgstr ""

#: stty.1.xml:453(para)
msgid "use delete characters for fill instead of null characters"
