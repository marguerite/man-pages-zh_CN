#: stty.1.xml:427(term)
msgid "* crN"
msgstr ""

#: stty.1.xml:429(para)
msgid "carriage return delay style, N in [0..3]"
msgstr ""

#: stty.1.xml:433(term)
msgid "* ffN"
msgstr ""

#: stty.1.xml:435(para)
