msgid ""
"<option>-F</option>, <option>--file</option>=<emphasis remap=\"I\">DEVICE</"
"emphasis>"
msgstr ""

#: stty.1.xml:60(para)
msgid "open and use the specified DEVICE instead of stdin"
msgstr ""

