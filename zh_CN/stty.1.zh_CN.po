msgid "--help"
msgstr ""

#: stty.1.xml:66(para)
msgid "display this help and exit"
msgstr ""

#: stty.1.xml:70(option)
msgid "--version"
msgstr ""

#: stty.1.xml:72(para)
msgid "output version information and exit"
msgstr ""

#: stty.1.xml:74(para)
msgid ""
"Optional - before SETTING indicates negation. An * marks non-POSIX settings. "
"The underlying system defines which settings are available."
msgstr ""

#: stty.1.xml:80(title)
msgid "Special characters:"
msgstr ""

#: stty.1.xml:83(term)
msgid "* dsusp CHAR"
msgstr ""

#: stty.1.xml:85(para)
msgid "CHAR will send a terminal stop signal once input flushed"
msgstr ""

#: stty.1.xml:89(term)
msgid "eof CHAR"
msgstr ""

#: stty.1.xml:91(para)
msgid "CHAR will send an end of file (terminate the input)"
msgstr ""

#: stty.1.xml:95(term)
msgid "eol CHAR"
msgstr ""

#: stty.1.xml:97(para)
msgid "CHAR will end the line"
msgstr ""

#: stty.1.xml:101(term)
msgid "* eol2 CHAR"
msgstr ""

#: stty.1.xml:103(para)
msgid "alternate CHAR for ending the line"
msgstr ""

#: stty.1.xml:107(term)
msgid "erase CHAR"
msgstr ""

#: stty.1.xml:109(para)
msgid "CHAR will erase the last character typed"
msgstr ""

#: stty.1.xml:113(term)
msgid "intr CHAR"
msgstr ""

#: stty.1.xml:115(para)
msgid "CHAR will send an interrupt signal"
msgstr ""

#: stty.1.xml:119(term)
msgid "kill CHAR"
msgstr ""

#: stty.1.xml:121(para)
msgid "CHAR will erase the current line"
msgstr ""

#: stty.1.xml:125(term)
msgid "* lnext CHAR"
msgstr ""

#: stty.1.xml:127(para)
msgid "CHAR will enter the next character quoted"
msgstr ""

#: stty.1.xml:131(term)
msgid "quit CHAR"
msgstr ""

#: stty.1.xml:133(para)
msgid "CHAR will send a quit signal"
msgstr ""

#: stty.1.xml:137(term)
msgid "* rprnt CHAR"
msgstr ""

#: stty.1.xml:139(para)
msgid "CHAR will redraw the current line"
msgstr ""

#: stty.1.xml:143(term)
msgid "start CHAR"
msgstr ""

#: stty.1.xml:145(para)
msgid "CHAR will restart the output after stopping it"
msgstr ""

#: stty.1.xml:149(term)
msgid "stop CHAR"
msgstr ""

#: stty.1.xml:151(para)
msgid "CHAR will stop the output"
msgstr ""

#: stty.1.xml:155(term)
msgid "susp CHAR"
msgstr ""

#: stty.1.xml:157(para)
msgid "CHAR will send a terminal stop signal"
msgstr ""

#: stty.1.xml:161(term)
msgid "* swtch CHAR"
msgstr ""

#: stty.1.xml:163(para)
msgid "CHAR will switch to a different shell layer"
msgstr ""

#: stty.1.xml:167(term)
msgid "* werase CHAR"
msgstr ""

#: stty.1.xml:169(para)
msgid "CHAR will erase the last word typed"
msgstr ""

#: stty.1.xml:175(title)
msgid "Special settings:"
msgstr ""

#: stty.1.xml:178(term)
msgid "N"
msgstr ""

#: stty.1.xml:180(para)
msgid "set the input and output speeds to N bauds"
msgstr ""

#: stty.1.xml:184(term)
msgid "* cols N"
msgstr ""

#: stty.1.xml:186(para)
msgid "tell the kernel that the terminal has N columns"
msgstr ""

#: stty.1.xml:190(term)
msgid "* columns N"
msgstr ""

#: stty.1.xml:192(para)
msgid "same as cols N"
msgstr ""

#: stty.1.xml:196(term)
msgid "ispeed N"
msgstr ""

#: stty.1.xml:198(para)
msgid "set the input speed to N"
msgstr ""

#: stty.1.xml:202(term)
msgid "* line N"
msgstr ""

#: stty.1.xml:204(para)
msgid "use line discipline N"
msgstr ""

#: stty.1.xml:208(term)
msgid "min N"
msgstr ""

#: stty.1.xml:210(para)
msgid ""
"with <option>-icanon</option>, set N characters minimum for a completed read"
msgstr ""

#: stty.1.xml:214(term)
msgid "ospeed N"
msgstr ""

#: stty.1.xml:216(para)
msgid "set the output speed to N"
msgstr ""

#: stty.1.xml:220(term)
msgid "* rows N"
msgstr ""

#: stty.1.xml:222(para)
msgid "tell the kernel that the terminal has N rows"
msgstr ""

#: stty.1.xml:226(term)
msgid "* size"
msgstr ""

#: stty.1.xml:228(para)
msgid "print the number of rows and columns according to the kernel"
msgstr ""

#: stty.1.xml:232(term)
msgid "speed"
msgstr ""

#: stty.1.xml:234(para)
msgid "print the terminal speed"
msgstr ""

#: stty.1.xml:238(term)
msgid "time N"
msgstr ""

#: stty.1.xml:240(para)
msgid "with <option>-icanon</option>, set read timeout of N tenths of a second"
msgstr ""

#: stty.1.xml:246(title)
msgid "Control settings:"
msgstr ""

#: stty.1.xml:249(term)
msgid "[-]clocal"
msgstr ""

#: stty.1.xml:251(para)
msgid "disable modem control signals"
msgstr ""

#: stty.1.xml:255(term)
msgid "[-]cread"
msgstr ""

#: stty.1.xml:257(para)
msgid "allow input to be received"
msgstr ""

#: stty.1.xml:261(term)
msgid "* [-]crtscts"
msgstr ""

#: stty.1.xml:263(para)
msgid "enable RTS/CTS handshaking"
msgstr ""

#: stty.1.xml:267(term)
msgid "* [-]cdtrdsr"
msgstr ""

#: stty.1.xml:269(para)
msgid "enable DTR/DSR handshaking"
msgstr ""

#: stty.1.xml:273(term)
msgid "csN"
msgstr ""

#: stty.1.xml:275(para)
msgid "set character size to N bits, N in [5..8]"
msgstr ""

#: stty.1.xml:279(term)
msgid "[-]cstopb"
msgstr ""

#: stty.1.xml:281(para)
msgid "use two stop bits per character (one with '-')"
msgstr ""

#: stty.1.xml:285(term)
msgid "[-]hup"
msgstr ""

#: stty.1.xml:287(para)
msgid "send a hangup signal when the last process closes the tty"
msgstr ""

#: stty.1.xml:291(term)
msgid "[-]hupcl"
msgstr ""

#: stty.1.xml:293(para)
msgid "same as [-]hup"
msgstr ""

#: stty.1.xml:297(term)
msgid "[-]parenb"
msgstr ""

#: stty.1.xml:299(para)
msgid "generate parity bit in output and expect parity bit in input"
msgstr ""

#: stty.1.xml:303(term)
msgid "[-]parodd"
msgstr ""

#: stty.1.xml:305(para)
msgid "set odd parity (or even parity with '-')"
msgstr ""

#: stty.1.xml:309(term)
msgid "* [-]cmspar"
msgstr ""

#: stty.1.xml:311(para)
msgid "use \"stick\" (mark/space) parity"
msgstr ""

#: stty.1.xml:317(title)
msgid "Input settings:"
msgstr ""

#: stty.1.xml:320(term)
msgid "[-]brkint"
msgstr ""

#: stty.1.xml:322(para)
msgid "breaks cause an interrupt signal"
msgstr ""

#: stty.1.xml:326(term)
msgid "[-]icrnl"
msgstr ""

#: stty.1.xml:328(para) stty.1.xml:447(para)
msgid "translate carriage return to newline"
msgstr ""

#: stty.1.xml:332(term)
msgid "[-]ignbrk"
msgstr ""

#: stty.1.xml:334(para)
msgid "ignore break characters"
msgstr ""

#: stty.1.xml:338(term)
msgid "[-]igncr"
msgstr ""

#: stty.1.xml:340(para)
msgid "ignore carriage return"
msgstr ""

#: stty.1.xml:344(term)
msgid "[-]ignpar"
msgstr ""

#: stty.1.xml:346(para)
msgid "ignore characters with parity errors"
msgstr ""

#: stty.1.xml:350(term)
msgid "* [-]imaxbel"
msgstr ""

#: stty.1.xml:352(para)
msgid "beep and do not flush a full input buffer on a character"
msgstr ""

#: stty.1.xml:356(term)
msgid "[-]inlcr"
msgstr ""

#: stty.1.xml:358(para)
msgid "translate newline to carriage return"
msgstr ""

#: stty.1.xml:362(term)
msgid "[-]inpck"
msgstr ""

#: stty.1.xml:364(para)
msgid "enable input parity checking"
msgstr ""

#: stty.1.xml:368(term)
msgid "[-]istrip"
msgstr ""

#: stty.1.xml:370(para)
msgid "clear high (8th) bit of input characters"
msgstr ""

#: stty.1.xml:374(term)
msgid "* [-]iutf8"
msgstr ""

#: stty.1.xml:376(para)
msgid "assume input characters are UTF-8 encoded"
msgstr ""

#: stty.1.xml:380(term)
msgid "* [-]iuclc"
msgstr ""

#: stty.1.xml:382(para)
msgid "translate uppercase characters to lowercase"
msgstr ""

#: stty.1.xml:386(term)
msgid "* [-]ixany"
msgstr ""

#: stty.1.xml:388(para)
msgid "let any character restart output, not only start character"
msgstr ""

#: stty.1.xml:392(term)
msgid "[-]ixoff"
msgstr ""

#: stty.1.xml:394(para)
msgid "enable sending of start/stop characters"
msgstr ""

#: stty.1.xml:398(term)
msgid "[-]ixon"
msgstr ""

#: stty.1.xml:400(para)
msgid "enable XON/XOFF flow control"
msgstr ""

#: stty.1.xml:404(term)
msgid "[-]parmrk"
msgstr ""

#: stty.1.xml:406(para)
msgid "mark parity errors (with a 255-0-character sequence)"
msgstr ""

#: stty.1.xml:410(term)
msgid "[-]tandem"
msgstr ""

#: stty.1.xml:412(para)
msgid "same as [-]ixoff"
msgstr ""

#: stty.1.xml:418(title)
msgid "Output settings:"
msgstr ""

#: stty.1.xml:421(term)
