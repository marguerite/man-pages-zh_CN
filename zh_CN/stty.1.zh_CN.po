#: stty.1.xml:481(term)
msgid "* [-]onocr"
msgstr ""

#: stty.1.xml:483(para)
msgid "do not print carriage returns in the first column"
msgstr ""

#: stty.1.xml:487(term)
msgid "[-]opost"
msgstr ""

#: stty.1.xml:489(para)
