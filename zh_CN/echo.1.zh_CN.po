#: echo.1.xml:8(refentrytitle)
msgid "ECHO"
msgstr "echo"

#: echo.1.xml:9(manvolnum)
msgid "1"
msgstr "1"

#: echo.1.xml:10(refmiscinfo)
msgid "July 2014"
msgstr "2014 年 7 月"

#: echo.1.xml:11(refmiscinfo)
msgid "GNU coreutils 8.22"
msgstr "GNU coreutils 8.22"

#: echo.1.xml:14(refname) echo.1.xml:20(command) echo.1.xml:26(command)
msgid "echo"
msgstr "echo"

#: echo.1.xml:15(refpurpose)
msgid "display a line of text"
msgstr "显示一行文本"

#: echo.1.xml:21(replaceable)
msgid "SHORT-OPTION"
msgstr "短选项"

#: echo.1.xml:22(replaceable)
msgid "STRING"
msgstr "字符串"

#: echo.1.xml:27(replaceable)
msgid "LONG-OPTION"
msgstr "长选项"

#: echo.1.xml:32(title)
msgid "DESCRIPTION"
msgstr "描述"

#. Add any additional description here
#: echo.1.xml:35(para)
msgid "Echo the STRING(s) to standard output."
msgstr "输出字符串到标准输出。"

#: echo.1.xml:38(option)
msgid "-n"
msgstr "-n"

#: echo.1.xml:40(para)
msgid "do not output the trailing newline"
msgstr "不输出结尾的新行符"

#: echo.1.xml:44(option)
msgid "-e"
msgstr "-e"

#: echo.1.xml:46(para)
msgid "enable interpretation of backslash escapes"
msgstr "启用反斜线逃逸符转义"

#: echo.1.xml:50(option)
msgid "-E"
msgstr "-E"

#: echo.1.xml:52(para)
msgid "disable interpretation of backslash escapes (default)"
msgstr "仅用反斜线逃逸符转义 (默认)"

#: echo.1.xml:56(option)
msgid "--help"
msgstr "--help"

#: echo.1.xml:58(para)
msgid "display this help and exit"
msgstr "显示此帮助并退出"

#: echo.1.xml:62(option)
msgid "--version"
msgstr "--version"

#: echo.1.xml:64(para)
msgid "output version information and exit"
msgstr "输出版本信息并退出"

#: echo.1.xml:66(para)
msgid ""
"If <option>-e</option> is in effect, the following sequences are recognized:"
msgstr "若 <option>-e</option> 生效了，则可识别以下序列："

#: echo.1.xml:72(para)
msgid "backslash"
msgstr "反斜线"

#: echo.1.xml:76(term)
msgid "&bsol;a"
msgstr "&bsol;a"

#: echo.1.xml:78(para)
msgid "alert (BEL)"
msgstr "警报 (响铃)"

#: echo.1.xml:82(term)
msgid "&bsol;b"
msgstr "&bsol;b"

#: echo.1.xml:84(para)
msgid "backspace"
msgstr "退格"

#: echo.1.xml:88(term)
msgid "&bsol;c"
msgstr "&bsol;c"

#: echo.1.xml:90(para)
msgid "produce no further output"
msgstr "不产生进一步输出"

#: echo.1.xml:94(term)
msgid "&bsol;e"
msgstr "&bsol;e"

#: echo.1.xml:96(para)
msgid "escape"
msgstr "Esc"

#: echo.1.xml:100(term)
msgid "&bsol;f"
msgstr "&bsol;f"

#: echo.1.xml:102(para)
msgid "form feed"
msgstr "翻页"

#: echo.1.xml:106(term)
msgid "&bsol;n"
msgstr "&bsol;n"

#: echo.1.xml:108(para)
msgid "new line"
msgstr "新行"

#: echo.1.xml:112(term)
msgid "&bsol;r"
msgstr "&bsol;r"

#: echo.1.xml:114(para)
msgid "carriage return"
msgstr "回车"

#: echo.1.xml:118(term)
msgid "&bsol;t"
msgstr "&bsol;t"

#: echo.1.xml:120(para)
msgid "horizontal tab"
msgstr "水平制表符"

#: echo.1.xml:124(term)
msgid "&bsol;v"
msgstr "&bsol;v"

#: echo.1.xml:126(para)
msgid "vertical tab"
msgstr "垂直制表符"

#: echo.1.xml:130(term)
msgid "&bsol;0NNN"
msgstr "&bsol;0NNN"

#: echo.1.xml:132(para)
msgid "byte with octal value NNN (1 to 3 digits)"
msgstr "八进制值为 NNN 的字节 (1 到 3 位)"

#: echo.1.xml:136(term)
msgid "&bsol;xHH"
msgstr "&bsol;xHH"

#: echo.1.xml:138(para)
msgid "byte with hexadecimal value HH (1 to 2 digits)"
msgstr "十六进制值为 HH 的字节 (1 到 2 位)"

#: echo.1.xml:140(para)
msgid ""
"NOTE: your shell may have its own version of echo, which usually supersedes "
"the version described here. Please refer to your shell's documentation for "
"details about the options it supports."
msgstr ""
"注意：您的壳层可能有其自有版本的 echo，其通常会覆盖这里的版本。"
"关于您壳层支持的选项的细节请参考其文档。"

#: echo.1.xml:144(para)
msgid ""
"GNU coreutils online help: &lt;<ulink url=\"http://www.gnu.org/software/"
"coreutils/&gt;\">http://www.gnu.org/software/coreutils/&gt;</ulink> Report "
"echo translation bugs to &lt;<ulink url=\"http://translationproject.org/team/"
"&gt;\">http://translationproject.org/team/&gt;</ulink>"
msgstr ""
"GNU coreutils 在线帮助：&lt;<ulink url=\"http://www.gnu.org/software/"
"coreutils/&gt;\">http://www.gnu.org/software/coreutils/&gt;</ulink> 汇报 "
"echo 翻译故障 &lt;<ulink url=\"http://translationproject.org/"
"team/&gt;\">http://translationproject.org/team/&gt;</ulink>"

#: echo.1.xml:151(title)
msgid "AUTHOR"
msgstr "作者"

#: echo.1.xml:152(para)
msgid "Written by Brian Fox and Chet Ramey."
msgstr "由 Brian Fox 和 Chet Ramey 编纂。"

