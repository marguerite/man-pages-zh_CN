#. Add any additional description here
#: fmt.1.xml:32(para)
msgid ""
"Reformat each paragraph in the FILE(s), writing to standard output. The "
"option <option>-WIDTH</option> is an abbreviated form of <option>--width</"
"option>=<emphasis remap=\"I\">DIGITS</emphasis>."
msgstr ""

#: fmt.1.xml:35(para)
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr ""

#: fmt.1.xml:38(term)
msgid "<option>-c</option>, <option>--crown-margin</option>"
msgstr ""

#: fmt.1.xml:40(para)
msgid "preserve indentation of first two lines"
msgstr ""

#: fmt.1.xml:44(term)
msgid ""
"<option>-p</option>, <option>--prefix</option>=<emphasis remap=\"I\">STRING</"
"emphasis>"
msgstr ""

#: fmt.1.xml:46(para)
msgid ""
"reformat only lines beginning with STRING, reattaching the prefix to "
"reformatted lines"
msgstr ""

#: fmt.1.xml:51(term)
msgid "<option>-s</option>, <option>--split-only</option>"
msgstr ""

#: fmt.1.xml:53(para)
msgid "split long lines, but do not refill"
msgstr ""

#: fmt.1.xml:57(term)
msgid "<option>-t</option>, <option>--tagged-paragraph</option>"
msgstr ""

#: fmt.1.xml:59(para)
msgid "indentation of first line different from second"
msgstr ""

#: fmt.1.xml:63(term)
msgid "<option>-u</option>, <option>--uniform-spacing</option>"
msgstr ""

#: fmt.1.xml:65(para)
msgid "one space between words, two after sentences"
msgstr ""

#: fmt.1.xml:69(term)
msgid ""
"<option>-w</option>, <option>--width</option>=<emphasis remap=\"I\">WIDTH</"
"emphasis>"
msgstr ""

#: fmt.1.xml:71(para)
msgid "maximum line width (default of 75 columns)"
msgstr ""

#: fmt.1.xml:75(term)
msgid ""
"<option>-g</option>, <option>--goal</option>=<emphasis remap=\"I\">WIDTH</"
"emphasis>"
msgstr ""

#: fmt.1.xml:77(para)
msgid "goal width (default of 93% of width)"
msgstr ""

#: fmt.1.xml:81(option)
msgid "--help"
msgstr ""

#: fmt.1.xml:83(para)
msgid "display this help and exit"
msgstr ""

#: fmt.1.xml:87(option)
msgid "--version"
msgstr ""

#: fmt.1.xml:89(para)
msgid "output version information and exit"
msgstr ""

#: fmt.1.xml:91(para)
msgid "With no FILE, or when FILE is -, read standard input."
msgstr ""

