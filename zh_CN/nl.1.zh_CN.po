#: nl.1.xml:28(title)
msgid "DESCRIPTION"
msgstr ""

#. Add any additional description here
#: nl.1.xml:31(para)
msgid ""
"Write each FILE to standard output, with line numbers added. With no FILE, "
"or when FILE is -, read standard input."
msgstr ""

#: nl.1.xml:34(para)
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr ""

#: nl.1.xml:37(term)
msgid ""
"<option>-b</option>, <option>--body-numbering</option>=<emphasis remap=\"I"
"\">STYLE</emphasis>"
msgstr ""

#: nl.1.xml:39(para)
msgid "use STYLE for numbering body lines"
msgstr ""

#: nl.1.xml:43(term)
msgid ""
"<option>-d</option>, <option>--section-delimiter</option>=<emphasis remap=\"I"
"\">CC</emphasis>"
msgstr ""

#: nl.1.xml:45(para)
msgid "use CC for separating logical pages"
msgstr ""

#: nl.1.xml:49(term)
msgid ""
"<option>-f</option>, <option>--footer-numbering</option>=<emphasis remap=\"I"
"\">STYLE</emphasis>"
msgstr ""

#: nl.1.xml:51(para)
msgid "use STYLE for numbering footer lines"
msgstr ""

#: nl.1.xml:55(term)
msgid ""
"<option>-h</option>, <option>--header-numbering</option>=<emphasis remap=\"I"
"\">STYLE</emphasis>"
msgstr ""

#: nl.1.xml:57(para)
msgid "use STYLE for numbering header lines"
msgstr ""

#: nl.1.xml:61(term)
msgid ""
"<option>-i</option>, <option>--line-increment</option>=<emphasis remap=\"I"
"\">NUMBER</emphasis>"
msgstr ""

#: nl.1.xml:63(para)
msgid "line number increment at each line"
msgstr ""

#: nl.1.xml:67(term)
msgid ""
"<option>-l</option>, <option>--join-blank-lines</option>=<emphasis remap=\"I"
"\">NUMBER</emphasis>"
msgstr ""

#: nl.1.xml:69(para)
msgid "group of NUMBER empty lines counted as one"
msgstr ""

#: nl.1.xml:73(term)
msgid ""
"<option>-n</option>, <option>--number-format</option>=<emphasis remap=\"I"
"\">FORMAT</emphasis>"
msgstr ""

#: nl.1.xml:75(para)
msgid "insert line numbers according to FORMAT"
msgstr ""

#: nl.1.xml:79(term)
msgid "<option>-p</option>, <option>--no-renumber</option>"
msgstr ""

#: nl.1.xml:81(para)
msgid "do not reset line numbers at logical pages"
msgstr ""

#: nl.1.xml:85(term)
msgid ""
"<option>-s</option>, <option>--number-separator</option>=<emphasis remap=\"I"
"\">STRING</emphasis>"
msgstr ""

#: nl.1.xml:87(para)
msgid "add STRING after (possible) line number"
msgstr ""

#: nl.1.xml:91(term)
msgid ""
"<option>-v</option>, <option>--starting-line-number</option>=<emphasis remap="
"\"I\">NUMBER</emphasis>"
msgstr ""

#: nl.1.xml:93(para)
msgid "first line number on each logical page"
msgstr ""

#: nl.1.xml:97(term)
msgid ""
"<option>-w</option>, <option>--number-width</option>=<emphasis remap=\"I"
"\">NUMBER</emphasis>"
msgstr ""

#: nl.1.xml:99(para)
msgid "use NUMBER columns for line numbers"
msgstr ""

#: nl.1.xml:103(option)
msgid "--help"
msgstr ""

#: nl.1.xml:105(para)
msgid "display this help and exit"
msgstr ""

#: nl.1.xml:109(option)
msgid "--version"
msgstr ""

#: nl.1.xml:111(para)
msgid "output version information and exit"
msgstr ""

#: nl.1.xml:113(para)
msgid ""
"By default, selects <option>-v1</option><option>-i1</option><option>-l1</"
"option><option>-sTAB</option><option>-w6</option><option>-nrn</"
"option><option>-hn</option><option>-bt</option><option>-fn</option>. CC are "
"two delimiter characters for separating logical pages, a missing second "
"character implies :. Type &bsol;&bsol; for &bsol;. STYLE is one of:"
msgstr ""

#: nl.1.xml:119(term)
msgid "a"
msgstr ""

#: nl.1.xml:121(para)
msgid "number all lines"
msgstr ""

#: nl.1.xml:125(term)
msgid "t"
msgstr ""

#: nl.1.xml:127(para)
msgid "number only nonempty lines"
msgstr ""

#: nl.1.xml:131(term)
msgid "n"
msgstr ""

#: nl.1.xml:133(para)
msgid "number no lines"
msgstr ""

#: nl.1.xml:137(term)
msgid "pBRE"
msgstr ""

#: nl.1.xml:139(para)
msgid ""
"number only lines that contain a match for the basic regular expression, BRE"
msgstr ""

#: nl.1.xml:142(para)
msgid "FORMAT is one of:"
msgstr ""

#: nl.1.xml:146(term)
msgid "ln"
msgstr ""

#: nl.1.xml:148(para)
msgid "left justified, no leading zeros"
msgstr ""

#: nl.1.xml:152(term)
msgid "rn"
msgstr ""

#: nl.1.xml:154(para)
msgid "right justified, no leading zeros"
msgstr ""

#: nl.1.xml:158(term)
msgid "rz"
msgstr ""

#: nl.1.xml:160(para)
msgid "right justified, leading zeros"
msgstr ""

