#: ptx.1.xml:8(refentrytitle)
msgid "PTX"
msgstr ""

#: ptx.1.xml:9(manvolnum)
msgid "1"
msgstr ""

#: ptx.1.xml:10(refmiscinfo)
msgid "July 2014"
msgstr ""

#: ptx.1.xml:11(refmiscinfo)
msgid "GNU coreutils 8.22"
msgstr ""

#: ptx.1.xml:14(refname) ptx.1.xml:22(command)
msgid "ptx"
msgstr ""

#: ptx.1.xml:15(refpurpose)
msgid "produce a permuted index of file contents"
msgstr ""

#: ptx.1.xml:19(para)
msgid ""
"<command>ptx</command> [<emphasis remap=\"I\">OPTION</emphasis>]... "
"[<emphasis remap=\"I\">INPUT</emphasis>]... <emphasis remap=\"I\">(without -"
"G)</emphasis>"
msgstr ""

#: ptx.1.xml:23(arg)
msgid "-G"
msgstr ""

#: ptx.1.xml:24(replaceable)
msgid "OPTION"
msgstr ""

#: ptx.1.xml:25(replaceable)
msgid "INPUT"
msgstr ""

#: ptx.1.xml:25(replaceable)
msgid "OUTPUT"
msgstr ""

#: ptx.1.xml:30(title)
msgid "DESCRIPTION"
msgstr ""

#. Add any additional description here
#: ptx.1.xml:33(para)
msgid ""
"Output a permuted index, including context, of the words in the input files."
msgstr ""

#: ptx.1.xml:35(para)
msgid ""
"Mandatory arguments to long options are mandatory for short options too."
msgstr ""

#: ptx.1.xml:38(term)
msgid "<option>-A</option>, <option>--auto-reference</option>"
msgstr ""

#: ptx.1.xml:40(para)
msgid "output automatically generated references"
msgstr ""

#: ptx.1.xml:44(term)
msgid "<option>-G</option>, <option>--traditional</option>"
msgstr ""

#: ptx.1.xml:46(para)
msgid "behave more like System V 'ptx'"
msgstr ""

#: ptx.1.xml:50(term)
msgid ""
"<option>-F</option>, <option>--flag-truncation</option>=<emphasis remap=\"I"
"\">STRING</emphasis>"
msgstr ""

#: ptx.1.xml:52(para)
msgid "use STRING for flagging line truncations"
msgstr ""

#: ptx.1.xml:56(term)
msgid ""
"<option>-M</option>, <option>--macro-name</option>=<emphasis remap=\"I"
"\">STRING</emphasis>"
msgstr ""

#: ptx.1.xml:58(para)
msgid "macro name to use instead of 'xx'"
msgstr ""

#: ptx.1.xml:62(term)
msgid ""
"<option>-O</option>, <option>--format</option>=<emphasis remap=\"I\">roff</"
"emphasis>"
msgstr ""

#: ptx.1.xml:64(para)
msgid "generate output as roff directives"
msgstr ""

#: ptx.1.xml:68(term)
msgid "<option>-R</option>, <option>--right-side-refs</option>"
msgstr ""

#: ptx.1.xml:70(para)
msgid "put references at right, not counted in <option>-w</option>"
msgstr ""

#: ptx.1.xml:74(term)
msgid ""
"<option>-S</option>, <option>--sentence-regexp</option>=<emphasis remap=\"I"
"\">REGEXP</emphasis>"
msgstr ""

#: ptx.1.xml:76(para)
msgid "for end of lines or end of sentences"
msgstr ""

#: ptx.1.xml:80(term)
msgid ""
"<option>-T</option>, <option>--format</option>=<emphasis remap=\"I\">tex</"
"emphasis>"
msgstr ""

#: ptx.1.xml:82(para)
msgid "generate output as TeX directives"
msgstr ""

#: ptx.1.xml:86(term)
msgid ""
"<option>-W</option>, <option>--word-regexp</option>=<emphasis remap=\"I"
"\">REGEXP</emphasis>"
msgstr ""

#: ptx.1.xml:88(para)
msgid "use REGEXP to match each keyword"
msgstr ""

#: ptx.1.xml:92(term)
msgid ""
"<option>-b</option>, <option>--break-file</option>=<emphasis remap=\"I"
"\">FILE</emphasis>"
msgstr ""

#: ptx.1.xml:94(para)
msgid "word break characters in this FILE"
msgstr ""

#: ptx.1.xml:98(term)
msgid "<option>-f</option>, <option>--ignore-case</option>"
msgstr ""

#: ptx.1.xml:100(para)
msgid "fold lower case to upper case for sorting"
msgstr ""

#: ptx.1.xml:104(term)
msgid ""
"<option>-g</option>, <option>--gap-size</option>=<emphasis remap=\"I"
"\">NUMBER</emphasis>"
msgstr ""

#: ptx.1.xml:106(para)
msgid "gap size in columns between output fields"
msgstr ""

#: ptx.1.xml:110(term)
msgid ""
"<option>-i</option>, <option>--ignore-file</option>=<emphasis remap=\"I"
"\">FILE</emphasis>"
msgstr ""

#: ptx.1.xml:112(para)
msgid "read ignore word list from FILE"
msgstr ""

#: ptx.1.xml:116(term)
msgid ""
"<option>-o</option>, <option>--only-file</option>=<emphasis remap=\"I"
"\">FILE</emphasis>"
msgstr ""

#: ptx.1.xml:118(para)
msgid "read only word list from this FILE"
msgstr ""

#: ptx.1.xml:122(term)
msgid "<option>-r</option>, <option>--references</option>"
msgstr ""

#: ptx.1.xml:124(para)
msgid "first field of each line is a reference"
msgstr ""

#: ptx.1.xml:126(para)
msgid ""
"<option>-t</option>, <option>--typeset-mode</option> - not implemented -"
msgstr ""

#: ptx.1.xml:130(term)
msgid ""
"<option>-w</option>, <option>--width</option>=<emphasis remap=\"I\">NUMBER</"
"emphasis>"
msgstr ""

#: ptx.1.xml:132(para)
msgid "output width in columns, reference excluded"
msgstr ""

#: ptx.1.xml:136(option)
msgid "--help"
msgstr ""

#: ptx.1.xml:138(para)
msgid "display this help and exit"
msgstr ""

#: ptx.1.xml:142(option)
msgid "--version"
msgstr ""

#: ptx.1.xml:144(para)
msgid "output version information and exit"
msgstr ""

#: ptx.1.xml:146(para)
msgid ""
"With no FILE, or when FILE is -, read standard input. Default is '-F /'."
msgstr ""

#: ptx.1.xml:148(para)
msgid ""
"GNU coreutils online help: &lt;<ulink url=\"http://www.gnu.org/software/"
"coreutils/&gt;\">http://www.gnu.org/software/coreutils/&gt;</ulink> Report "
"ptx translation bugs to &lt;<ulink url=\"http://translationproject.org/team/"
"&gt;\">http://translationproject.org/team/&gt;</ulink>"
msgstr ""

