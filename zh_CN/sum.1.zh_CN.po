#: sum.1.xml:8(refentrytitle)
msgid "SUM"
msgstr "sum"

#: sum.1.xml:9(manvolnum)
msgid "1"
msgstr "1"

#: sum.1.xml:10(refmiscinfo)
msgid "July 2014"
msgstr "2014 年 7 月"

#: sum.1.xml:11(refmiscinfo)
msgid "GNU coreutils 8.22"
msgstr "GNU coreutils 8.22"

#: sum.1.xml:14(refname) sum.1.xml:20(command)
msgid "sum"
msgstr "sum"

#: sum.1.xml:15(refpurpose)
msgid "checksum and count the blocks in a file"
msgstr "校验文件并统计其区块数"

#: sum.1.xml:21(replaceable)
msgid "OPTION"
msgstr "选项"

#: sum.1.xml:22(replaceable)
msgid "FILE"
msgstr "FILE"

#: sum.1.xml:28(title)
msgid "DESCRIPTION"
msgstr "描述"

#. Add any additional description here
#: sum.1.xml:31(para)
msgid "Print checksum and block counts for each FILE."
msgstr "输出每个 FILE 的校验值和区块数。"

#: sum.1.xml:34(option)
msgid "-r"
msgstr "-r"

#: sum.1.xml:36(para)
msgid "use BSD sum algorithm, use 1K blocks"
msgstr "使用 BSD 求和算法，使用 1K 区块"

#: sum.1.xml:40(term)
msgid "<option>-s</option>, <option>--sysv</option>"
msgstr "<option>-s</option>, <option>--sysv</option>"

#: sum.1.xml:42(para)
msgid "use System V sum algorithm, use 512 bytes blocks"
msgstr "使用 System V 算法，使用 512 字节区块"

#: sum.1.xml:46(option)
msgid "--help"
msgstr "--help"

#: sum.1.xml:48(para)
msgid "display this help and exit"
msgstr "显示此帮助并退出"

#: sum.1.xml:52(option)
msgid "--version"
msgstr "--version"

#: sum.1.xml:54(para)
msgid "output version information and exit"
msgstr "输出版本信息并退出"

#: sum.1.xml:56(para)
msgid "With no FILE, or when FILE is -, read standard input."
msgstr "若没有 FILE 或 FILE 是 -，则从标准输入读取。"

#: sum.1.xml:58(para)
msgid ""
"GNU coreutils online help: &lt;<ulink url=\"http://www.gnu.org/software/"
"coreutils/&gt;\">http://www.gnu.org/software/coreutils/&gt;</ulink> Report "
"sum translation bugs to &lt;<ulink url=\"http://translationproject.org/team/"
"&gt;\">http://translationproject.org/team/&gt;</ulink>"
msgstr ""
"GNU coreutils 在线帮助：&lt;<ulink url=\"http://www.gnu.org/software/"
"coreutils/&gt;\">http://www.gnu.org/software/coreutils/&gt;</ulink> 汇报 "
"sum 翻译故障 &lt;<ulink url=\"http://translationproject.org/"
"team/&gt;\">http://translationproject.org/team/&gt;</ulink>"

