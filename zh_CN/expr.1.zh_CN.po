#: expr.1.xml:69(para)
msgid "ARG1 is less than or equal to ARG2"
msgstr ""

#: expr.1.xml:73(term)
msgid "ARG1 = ARG2"
msgstr ""

#: expr.1.xml:75(para)
msgid "ARG1 is equal to ARG2"
msgstr ""

#: expr.1.xml:79(term)
msgid "ARG1 != ARG2"
msgstr ""

#: expr.1.xml:81(para)
