msgstr ""

#: expr.1.xml:103(term)
msgid "ARG1 - ARG2"
msgstr ""

#: expr.1.xml:105(para)
msgid "arithmetic difference of ARG1 and ARG2"
msgstr ""

#: expr.1.xml:109(term)
msgid "ARG1 * ARG2"
msgstr ""

#: expr.1.xml:111(para)
msgid "arithmetic product of ARG1 and ARG2"
msgstr ""

#: expr.1.xml:115(term)
msgid "ARG1 / ARG2"
msgstr ""

#: expr.1.xml:117(para)
msgid "arithmetic quotient of ARG1 divided by ARG2"
msgstr ""

#: expr.1.xml:121(term)
msgid "ARG1 % ARG2"
msgstr ""

#: expr.1.xml:123(para)
