msgid "ARG1 &amp; ARG2"
msgstr ""

#: expr.1.xml:57(para)
msgid "ARG1 if neither argument is null or 0, otherwise 0"
msgstr ""

#: expr.1.xml:61(term)
msgid "ARG1 &lt; ARG2"
msgstr ""

#: expr.1.xml:63(para)
msgid "ARG1 is less than ARG2"
msgstr ""

#: expr.1.xml:67(term)
msgid "ARG1 &lt;= ARG2"
