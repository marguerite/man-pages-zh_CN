#: rm.1.xml:8(refentrytitle)
msgid "RM"
msgstr "rm"

#: rm.1.xml:9(manvolnum)
msgid "1"
msgstr "1"

#: rm.1.xml:10(refmiscinfo)
msgid "July 2014"
msgstr "2014 年 7 月"

